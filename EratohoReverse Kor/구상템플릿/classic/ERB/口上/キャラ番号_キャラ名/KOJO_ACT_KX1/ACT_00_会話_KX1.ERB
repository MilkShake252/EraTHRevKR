;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL && !TFLAG:ACT파생
	;=================================================
	;初回
	;記入チェック
	LOCAL:1 = 1
	;=================================================
	IF LOCAL:1 && FIRSTACT(0)
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;조교중初回
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && ONCEACT(0)
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;죠교대상に페니스あり
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && PENIS(MASTER)
		;발기している
		IF TCVAR:MASTER:발기도 >= 1000
;OriginalString : 
			PRINTFORMW 
		;その他
		ELSE
;OriginalString : 
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;죠교대상に페니스なし
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && !PENIS(MASTER)
		;濡れてる
		IF PALAM:MASTER:윤활 > 500
;OriginalString : 
			PRINTFORMW 
		;その他
		ELSE
;OriginalString : 
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;その他・条件なし
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
ENDIF

