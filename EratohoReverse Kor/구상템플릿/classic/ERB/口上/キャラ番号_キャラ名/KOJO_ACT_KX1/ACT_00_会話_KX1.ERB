;地の文は"%CALLNAME:TARGET%は%CALLNAME:MASTER%を大声で怒鳴った…"のみで派生はない
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT8, 고함치다
;記入チェック（=0, 非表示、1, 表示、2, 表示して미약等구상をスキップ）
LOCAL = 1
;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL
	;=================================================
	;初回
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && FIRSTACT(0)
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;조교중初回
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && ONCEACT(0)
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;前COMが「무뚝뚝하게 응답한다/강하게 응답한다/싫어한다/날뛴다/거부한다」のいずれか
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_PREVCOMNAME("무뚝뚝하게 응답한다/강하게 응답한다/싫어한다/날뛴다/거부한다")
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;その他・条件なし
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT9, 기분나쁘게 웃다
;────────────────────────────────────
;・不気味な微笑を浮かべる
;・이상系から発生する場合もある
;・금일의방침が"イチャイチャ"だと発生しない
;・話題『嗤う』が既出となり、以後、同一조교중では抑制される
;-------------------------------------------------
@KOJO_ACT_KX1_9
;-------------------------------------------------
;関数@TARGET_DESCRIPTIONによってTEQUIP(성교중/죠교대상Ｃ사용/성교봉사중/자위중 & 2)で地の文は변화する
;その他・条件なしを最初に埋めた上で、あとは好みで分岐を増減してください
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT9, 기분나쁘게 웃다
;記入チェック（=0, 非表示、1, 表示、2, 表示して미약等구상をスキップ）
LOCAL = 0
;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL
	;=================================================
	;初回
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && FIRSTACT(0)
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;PENIS(TARGET)/!PENIS(TARGET)/TEQUIP:죠교자페니스밴드等で分岐してもよい
	;=================================================
	;성교중
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TEQUIP:성교중
		;体位による派生
		SELECTCASE TEQUIP:성교중
			;정상위				체중을 실어 %CALLNAME:MASTER%의 질내에 가득
			CASE 1
;OriginalString : 
				PRINTFORMW 
			;후배위				뒤에서 %CALLNAME:MASTER%의 질내에 가득
			CASE 2
;OriginalString : 
				PRINTFORMW 
			;기승위				%CALLNAME:MASTER%의 암컷 구멍에 끝까지 밀어넣어
			CASE 3
;OriginalString : 
				PRINTFORMW 
			;대면좌위			밀착한 %CALLNAME:MASTER%의 질을 애무하며
			CASE 4
;OriginalString : 
				PRINTFORMW 
			;배면좌위			%CALLNAME:MASTER%에게 삽입하여 껴안고
			CASE 5
;OriginalString : 
				PRINTFORMW 
			;항문섹스		%CALLNAME:MASTER%의 의 항문에 넣어
			CASE 6
;OriginalString : 
				PRINTFORMW 
		ENDSELECT
		RETURN LOCAL:1
	ENDIF
	
	;죠교대상のＣへの애무が行われている状況。TEQUIP관리なので可能性は割りとある
	;=================================================
	;죠교대상Ｃ사용
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && GROUPMATCH(TEQUIP:죠교대상Ｃ사용, 1, 2)
		;수음・애무継続中
		IF TEQUIP:죠교대상Ｃ사용 == 1
;OriginalString : 
			PRINTFORMW 
		;펠라・クンニ継続中
		ELSE
;OriginalString : 
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;PENIS(MASTER)/!PENIS(MASTER)/TEQUIP:페니스밴드等で分岐してもよい
	;=================================================
	;성교봉사중
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TEQUIP:성교봉사중
		;体位による派生
		SELECTCASE TEQUIP:성교봉사중
			;정상위시킨다				大きく開いた股の間に迎え入れ
			CASE 1
;OriginalString : 
				PRINTFORMW 
			;후배위시킨다				背後から질내に삽입され
			CASE 2
;OriginalString : 
				PRINTFORMW 
			;역강간					ぬるぬるの膣できゅうきゅうと絞り上げ
			CASE 3
;OriginalString : 
				PRINTFORMW 
			;대면좌위시킨다				体を密着させて膣奥に歓迎し
			CASE 4
;OriginalString : 
				PRINTFORMW 
			;배면좌위시킨다				背を預けて堪能し
			CASE 5
;OriginalString : 
				PRINTFORMW 
			;항문섹스시킨다		腸液にぬめる肛門で締め付け
			CASE 6
;OriginalString : 
				PRINTFORMW 
		ENDSELECT
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;죠교자が자위중
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && GETBIT(TEQUIP:자위중, 1)
		;죠교자が페니스밴드身に付けている
		IF TEQUIP:죠교자페니스밴드
;OriginalString : 
			PRINTFORMW
		;その他
		ELSE
;OriginalString : 
			PRINTFORMW
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;その他・条件なし
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
ENDIF
