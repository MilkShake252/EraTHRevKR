;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL && FIRSTACT(0)
	;=================================================
	;ACT파생が펠라強制
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TFLAG:ACT파생 == 1
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;ACT파생が안면승마
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TFLAG:ACT파생 == 2
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;ACT파생が안면승마항문
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TFLAG:ACT파생 == 3
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;上層の分岐を全て埋めた場合はクンニ強制扱いになる
	;派生毎に書き分けないのであれば、この箇所を汎用的に使える内容に했다上で不要な分岐を削除すると良いでしょう
	;=================================================
	;その他・条件なし
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
ENDIF

;죠교대상のＣを責めつつ안면승마になるシチュエーション
;안면승마になる場合は죠교자의 페니스、페니스밴드等を考慮する필요なし
;필요に応じて
;IF TEQUIP:페니스밴드
;ELSEIF PENIS(MASTER)
;ELSE
;等の分岐を設けて使用して下さい
;TEQUIP:죠교대상Ｃ사용 == 1は손으로 애무中
;TEQUIP:죠교대상Ｃ사용 == 2は입으로 애무中となります
;一応입と손それぞれの分岐を設けま했다が
;죠교자の素質によっては입으로 애무が殆ど出なかったりするので、そのあたりを見極めた上で分岐を増減して下さい
;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상がＣを責められている
;記入チェック
LOCAL = 0
;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL && GROUPMATCH(TEQUIP:죠교대상Ｃ사용, 1, 2) > 0
	;=================================================
	;질내사정後3ターン以内		膣입からは先程たっぷりと搾り取った정액がどろりとあふれでている…
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && INRANGE(TCVAR:MASTER:질내사정후경과턴, 1, 2)
		;입으로 애무中
		IF TEQUIP:죠교대상Ｃ사용 == 2
;OriginalString : 
			PRINTFORMW 
		;손으로 애무中
		ELSE
;OriginalString : 
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;안면승마항문派生			目の前にひくつく菊門がさらけ出されている……
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TFLAG:ACT파생 == 3
		;입으로 애무中
		IF TEQUIP:죠교대상Ｃ사용 == 2
;OriginalString : 
			PRINTFORMW 
		;손으로 애무中
		ELSE
;OriginalString : 
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;통상の안면승마を想定。@VAGINA_DESCRIPTIONは描写系関数
	;=================================================
	;その他・条件なし			無防備な%VAGINA_DESCRIPTION(TARGET)%がさらけ出されている……
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1
		;입으로 애무中
		IF TEQUIP:죠교대상Ｃ사용 == 2
;OriginalString : 
			PRINTFORMW 
		;손으로 애무中
		ELSE
;OriginalString : 
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
ENDIF

