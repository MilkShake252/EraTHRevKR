	;前から애무
	IF TCVAR:MASTER:위치전후 == 1
;OriginalString : 
		PRINTFORMW 
	;後ろから애무
	ELSE
;OriginalString : 
		PRINTFORMW 
	ENDIF
	RETURN 1
ENDIF

