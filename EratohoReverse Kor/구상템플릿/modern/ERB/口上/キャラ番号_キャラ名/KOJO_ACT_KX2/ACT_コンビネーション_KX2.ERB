	ENDIF
	
	;=================================================
	;성교봉사중
	;=================================================
	IF 구상記入チェック(0) && TEQUIP:성교봉사중
		;애태움中
		IF TCVAR:MASTER:애태움도 > 1
;OriginalString : 
			PRINTFORMW 
		;애태움無し
		ELSE
;OriginalString : 
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;その他・条件なし
	;=================================================
	IF 구상記入チェック(0)
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
