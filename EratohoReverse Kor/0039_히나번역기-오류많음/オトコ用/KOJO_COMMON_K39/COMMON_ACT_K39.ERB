;========================================================================
;汎用アクション구상 AUTORAND(500) ～
;========================================================================
;・個別の죠교자アクションが表示されなかった際に表示
;・@KOJO_ACT_KX以外で関数分けはされておらず、全て共通で呼び出される
;・この部分のみを使用し、コマンド毎に呼び出しを分けていな他のバリアント구상のような使い方も可能
;・元々は손間を減ら했다い場合のeasy用機能として設けられたもの
;・SELECTCASE GET_ACTNAME(TFLAG:ACT)で分けるもいいし、IS_NOWACTNAMEでIF ELSEIFで分けてもいい
;・さらに손間を減ら했다いならMENUMATCH等でACT분류で分けてもいい(基本的に非推奨だけど)
;・個別にRESULT返してもいいし、最後にまとめてRESULTを返してもいい
;・ACT毎の名称はStr.CSVの0～103を参照の事
;-------------------------------------------------
;・easy用구상テンプレートはこの汎用アクション(@KOJO_ACT)と
;      統括汎用リアクション(@KOJO_REACT)機能を基本として作成する予定
;-------------------------------------------------
@KOJO_MALE_ACT_K39
#LOCALSIZE 10
#LOCALSSIZE 10
VARSET LOCAL
VARSET LOCALS
RESULT = 0

;//이 구상은 통상のACT구상、"その他・条件なし"の箇所が確立で真になるようになっているので
;//そこから漏れた場合はこちらで処理されるようになっている
;//汎用ACTが実装당했다時に試験的にやり始めて、結局そのまま残ってるといかいうアレ
;//今更통상のACTに移し替えるのも面倒だし…

;//ACT系の最後の受け皿なので確率で真偽になる類の分岐は基本的に設けない
;//今後この箇所を손入れする可能性は非常に低い
;//上気の理由によりここでは記入チェックを使わない
;//条件毎の우선度の見極め、不要な部分の削除、コメントアウトが徹底できるなら、そもそも不要でもある

;────────────────────────────────────
;ACT10：애무
;────────────────────────────────────
IF IS_NOWACTNAME("손으로 애무")
	;수음する
	IF TFLAG:ACT파생 == 1
		IF 0
		;연속실행
		ELSEIF TFLAG:ACT == TFLAG:전ACT
			;상사상애
			IF GET_KFLAG_K39("상사상애")
				SELECTCASE AUTORAND(5, 500)
					CASE 0
						PRINTFORMW 「마음껏 격렬하고 시고 쉬는 것 정말 좋아하겠지？」
					CASE 1
						PRINTFORMW 「시고 그면서 유두를 만지작거려지는 것은 어때？」
					CASE 2
						PRINTFORMW 「타액 바를 수 있는의 손으로 끝(쪽)만 꾸짖을 수 있는 것은 어때？」
					CASE 3
						PRINTFORMW 「왼손으로 시고 쉬면(자) 조금 다른 느낌이겠지？」
					CASE 4
						PRINTFORMW 「함께 엉덩이의 구멍도 만지작거려지면(자) 감등 없지요？」
				ENDSELECT
				SELECTCASE AUTORAND(5, 501)
					CASE 0
						PRINTFORMW 「이크시는 분명하게 「이키！」(이)라고 말하지 않으면 안되어요？ 후후」
					CASE 1
						PRINTFORM 「손안으로 자꾸자꾸 커진다……어쩐지 나도 흥분해 와 버렸다
						CALL PRINT_SYMBOL_K39
					CASE 2
						PRINTFORM 「이키 싶어지면(자) 언제라도 사정해？ 이봐요 이봐요！ 빨리 이좋은 차이야
						CALL PRINT_SYMBOL_K39
					CASE 3
						PRINTFORMW 「이봐요, 좀 더 느껴,%TNAME_K39()%의 손으로 기분 좋아져…」
					CASE 4
						PRINTFORMW 「우대신, 벌벌 시켜 버려서. 그렇게 기분 좋은 것일까
						CALL PRINT_SYMBOL_K39
				ENDSELECT
			;その他
			ELSE
				SELECTCASE AUTORAND(6, 502)
					CASE 0
						PRINTFORMW 「벌벌 시켜 기분이야 유혹하는 것으로 무릎…」
					CASE 1
						PRINTFORMW 「좀 더 강하게 해에 시고 있는 것이 좋습니까？」
					CASE 2
						PRINTFORMW 「소리가 나오는 것을 억제할 수 없는 것 같네요？」
					CASE 3
						PRINTFORMW 「타액으로 적셔 첨단을 켜지는 것은 어떻습니까…」
					CASE 4
						PRINTFORMW 「역수로 시고 고난이라고 말하는 것은 어떻습니까？」
					CASE 5
						PRINTFORMW 「함께 유두를 만지작거려지는 것은 어떻습니까？」
				ENDSELECT
				SELECTCASE AUTORAND(4, 503)
					CASE 0
						PRINTFORMW 「이 오고 싶어지면(자) 언제라도 내도 좋아요♪」
					CASE 1
						PRINTFORMW 「, 액신의 손가락 다루기를 좀 더 즐겨 주세요♪」
					CASE 2
						PRINTFORMW 「허리가 자연스럽게 움직여 버리는 것도 멈출 수 없는 것 같네요？ 우대신」	
					CASEELSE
						PRINTFORMW 「이크시는 분명하게 나에게 말해 주세요？ 우대신」
				ENDSELECT
			ENDIF
			PRINTFORMDW %CALLNAME%(은)는 때때로%CALLNAME:MASTER%의 %RAND_SPLIT("유두를 만지작거려/엉덩이를 애무하기/음낭을 상냥하게 주물러 품/귀에 입김을 내뿜고")%(이)면서, 페니스를 계속 훑어 냈다…
			RETURN 1
		;その他：통상のアクション
		ELSE
			;상사상애
			IF GET_KFLAG_K39("상사상애")
				SELECTCASE AUTORAND(4, 505)
					CASE 0
						PRINTFORMW 「손으로 켰으면 좋아서 기다릴 수  없지요？ 이봐요, 정직하게 말하지 않으면 시라고 주지 않아？」
					CASE 1
						PRINTFORMW 「이봐요, 가만히 하고 있어？ 손코키로 승천 시켜 주기 때문에♪」
					CASE 2
						PRINTFORMW 「, 얌전하게 나에게 시고 그없음 있고？ 텅 비게 될 때까지 시라고 주기 때문에♪」
					CASE 3
						PRINTFORMW 「그런 눈으로 보지 않아도 알아요. 좋아, 손으로 시고 있어 준다♪」
				ENDSELECT
				SELECTCASE AUTORAND(4, 506)
					CASE 0
						PRINTFORMW 「변함 없이 좋은 반응이군요？ 응, 좀 더 강하고 시고 있는 것이 좋을까」
					CASE 1
						PRINTFORMW 「응, 간단하게 이는은 안되어요？ 조금은 즐기게 해. 우대신」
					CASE 2
						PRINTFORMW 「이런 느낌？ 그렇지 않으면 이러한 (분)편이 좋다？ 우대신」
					CASE 3
						PRINTFORMW 「벌써 끝(쪽)이 이렇게 누르눌인 것？ 이봐요, 좀 더 기분 좋게 해 준다」
				ENDSELECT
			;その他
			ELSE
				SELECTCASE AUTORAND(4, 507)
					CASE 0
						PRINTFORMW 「, 나의 손으로 녹여 주어요♪」
					CASE 1
						PRINTFORMW 「 나의 손으로 시고 쉬는 것이 기다릴 수  없습니까？」
					CASE 2
						PRINTFORMW 「벌써 이렇게 딱딱하게 해 버려……곧바로 편하게 해 줍니다♪」
					CASE 3
						PRINTFORMW 「기대하고 있겠지요？ 말하지 않아도 알아요」
				ENDSELECT
				SELECTCASE AUTORAND(4, 508)
					CASE 0
						PRINTFORMW 「이봐요, 이런 느낌은 어떻습니까. 어, 좀 더 강하고입니까？ 후후」
					CASE 1
						PRINTFORMW 「이봐요, 응시할 수 있는이면서 시고 쉬면(자) 흥분하지 않겠습니까？ 우대신」
					CASE 2
						PRINTFORMW 「꽤 잡아당겨 반응이 있네요…비울 때까지 시라고 주어요♪」
					CASE 3
						PRINTFORMW 「완전히 얌전해져 버려. 그렇게 좋을까요？ 우대신」
				ENDSELECT
			ENDIF
			PRINTFORMDW %CALLNAME%(은)는 %RAND_SPLIT("요염한 미소를 띄우면서/스스로의 애액을 도하면서/스스로의 타액을 도하면서/끈적했다시선을 관련되면서")%, 그 손으로 당신의 페니스를 훑어 내기 시작했다…
			RETURN 1
		ENDIF
	ENDIF
ENDIF

;────────────────────────────────────
;ACT파생11：가슴애무
;────────────────────────────────────
IF IS_NOWACTNAME("가슴애무")
	SELECTCASE TFLAG:ACT파생
		;乳首吸い
		CASE 1
			RETURN 0
		;揉み合い
		CASE 2
			RETURN 0
		;乳首責め
		CASE 3
			;상사상애
			IF GET_KFLAG_K39("상사상애")
				SELECTCASE AUTORAND(3, 510)
					CASE 0
						PRINTFORMW 「이봐요, 가만히 하고 있어？」
						PRINTFORMW 「마음껏 즐기게 해 주기 때문에♪」
					CASE 1
						PRINTFORMW 「응, 만지작거렸으면 좋지요？」
						PRINTFORMW 「솔직하게 말하지 않으면 시라고 주지 않으니까♪」
					CASE 2
						PRINTFORMW 「, 즐기게 해 준다」
						PRINTFORMW 「기대해도 좋아요？ 우대신」
				ENDSELECT
			;その他
			ELSE
				SELECTCASE AUTORAND(4, 511)
					CASE 0
						PRINTFORMW 「, 기분 좋게 해 주어요」
						PRINTFORMW 「안심해 나에게 맡겨 주세요♪」
					CASE 1
						PRINTFORMW 「얌전하게 하고 있어 주세요？」
						PRINTFORMW 「기분 좋게 해 줄 뿐입니다로부터♪」
					CASE 2
						PRINTFORMW 「우대신, 기대해도 좋아요」
						PRINTFORMW 「마음껏 즐기게 해 줄테니까♪」
					CASE 3
						PRINTFORMW 「당신은 어떤 느낌에 만지작거려지는 것을 좋아하는 것입니까？」
						PRINTFORMW 「이런 느낌？ 그렇지 않으면 이러할까요？ 우대신……」
				ENDSELECT
			ENDIF
			RETURN 1
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;────────────────────────────────────
;ACT파생12：입으로 애무
;────────────────────────────────────
IF IS_NOWACTNAME("입으로 애무")
	;ACT파생1：펠라
	IF TFLAG:ACT파생 == 1
		;연속실행
		IF TFLAG:ACT == TFLAG:전ACT
			;フェードアウト
			CALL CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K39(, , "펠라", 1)
			;상사상애
			IF GET_KFLAG_K39("상사상애")
				SELECTCASE AUTORAND(4, 520)
					CASE 0
						PRINTFORMW 「눈을 치켜 뜨고 봄으로 응시할 수 있어 빨아지는 것이라는 흥분해…？」
					CASE 1
						PRINTFORM 「이 기초가 되면 가르쳐 줘？ 전부 마셔 주기 때문에…
						CALL PRINT_SYMBOL_K39
					CASE 2
						PRINTFORM 「우대신, 두고 해♪ 이봐요, 좀 더 격렬하게 시라고 준다…
						CALL PRINT_SYMBOL_K39
					CASE 3
						PRINTFORM 「이봐요 , 움직이면(자) 잘 나오고 오지 않지요. 얌전하게 하고 있어…
						CALL PRINT_SYMBOL_K39
				ENDSELECT
				;終端でRETURN
			;その他
			ELSE
				SELECTCASE AUTORAND(4, 521)
					CASE 0
						PRINTFORM 「기분 좋습니까？ 우대신, 좀 더 녹여 줍니다…
						CALL PRINT_SYMBOL_K39
					CASE 1
						PRINTFORMW 「이 기초가 되면 말해 주세요？」
					CASE 2
						PRINTFORMW 「첨단의 뒤측을 혀로 찔리는 것이 좋은 것 같네요？」
					CASE 3
						PRINTFORM 「 아직 노력합니다？ 그럼, 조금 격렬하고 이 와요
						CALL PRINT_SYMBOL_K39
				ENDSELECT
				;終端でRETURN
			ENDIF
			;죠교자が"정욕"もしくは25%で真
			SIF IS_CONDITION(TARGET, "정욕") || !RAND:4
				CALL CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K39(, , "펠라", 1)
			RETURN 1
		;통상
		ELSE
			;상사상애
			IF GET_KFLAG_K39("상사상애")
				SELECTCASE AUTORAND(4, 522)
					CASE 0
						PRINTFORM 「입으로 시라고 갖고 싶지요？ 이봐요, 정직하게 말하세요…
						CALL PRINT_SYMBOL_K39
					CASE 1
						PRINTFORMW 「이봐요, 다리를 열어. 이것을 시라고 갖고 싶어서 실마리？ 우대신」
					CASE 2
						PRINTFORM 「우대신, 이야라시이 냄새군요. 조금 흥분해버린다
						CALL PRINT_SYMBOL_K39
					CASE 3
						PRINTFORMW 「, 입으로 녹여 준다. 준비는 좋다？」
				ENDSELECT
				;フェードアウト
				CALL CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K39(, , "펠라", 1)
;OriginalString : 「ん%RAND_SPLIT("っ/ッ")%……
				PRINT 「응%RAND_SPLIT("/")%……
				SELECTCASE AUTORAND(4, 523)
					CASE 0
						PRINTFORMW 아직도 이런 것은 미안해요？」
					CASE 1
						PRINTFORMW 응, 기분 좋다？ 어떻게야？」
					CASE 2
						PRINTFORMW 몹시, 뜨겁다……」
					CASE 3
						PRINTFORMW 자꾸자꾸 커져…」
				ENDSELECT
				;RETURNは終端で
			;その他
			ELSE
				SELECTCASE AUTORAND(4, 524)
					CASE 0
						PRINTFORMW 「말하지 않아도 알아요, 입으로 시라고 갖고 싶네요？」
					CASE 1
						PRINTFORMW 「, 액신의 혀사용을 마음껏 만끽해 주어요. 각오 해 주세요？」
					CASE 2
						PRINTFORM 「움직이지 말아 주세요？ 나의 입으로 짜 취해 줍니다%RAND_SPLIT("로부터/")%
						CALL PRINT_SYMBOL_K39
					CASE 3
						PRINTFORM 「이 냄새가 감등 없네요
						CALL AUTOHEARTCB2_K39
						PRINTFORMW  , 준비는 좋습니까？」
				ENDSELECT
				CALL CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K39(, , "펠라", 1)
;OriginalString : 「ん%RAND_SPLIT("っ/ッ")%……」
				PRINT 「응%RAND_SPLIT("/")%……」
				SELECTCASE AUTORAND(4, 525)
					CASE 0
						PRINTFORM 지금부터가 실전이에요…
						CALL PRINT_SYMBOL_K39
					CASE 1
						PRINTFORMW 즐겨 줄 수 있습니까？」
					CASE 2
						PRINTFORMW 몹시, 뜨거워서……」
					CASE 3
						PRINTFORMW 입 중(안)에서, 이렇게 크고…」
				ENDSELECT
				;RETURNは終端で
			ENDIF
			PRINTFORMDW %CALLNAME%(은)는 %RAND_SPLIT("귀두에 혀를 걸면서/눈을 치켜 뜨고 봄으로 올려보면서/손그리고 근원을 비비면서/편손그리고 음낭을 상냥하게 비비어 해하면서")%, 당신의 페니스를 입으로 훑어 내기 시작했다…
			RETURN 1
		ENDIF
	ENDIF
ENDIF

