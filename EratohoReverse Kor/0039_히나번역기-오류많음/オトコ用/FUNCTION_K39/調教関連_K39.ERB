		RETURNF LOCAL == 5
	CASE "가슴"
		RETURNF LOCAL == 6
	CASE "발"
		RETURNF LOCAL == 7
	CASE "膣입"
		RETURNF LOCAL == 8
	CASE "로터"
		RETURNF LOCAL == 100
	CASE "클리캡"
		RETURNF LOCAL == 112
	CASE "오나홀"
		RETURNF LOCAL == 114
	CASEELSE
		CALLF PRINT_ERRORMESSAGE("@MC_CHECK_K39に無効な定義が渡されま했다")
