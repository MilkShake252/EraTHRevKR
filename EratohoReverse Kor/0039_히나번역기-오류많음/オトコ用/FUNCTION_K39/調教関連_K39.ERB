;-------------------------------------------------
;関数名:GET_EX_K39
;概　要:文字列でEX関連を参照する
;引　数:ARGS…절정の種類
;戻り値:指定했다文字列に準じたNOWEX
;使用例:GET_EX_K39("MC절정")
;-------------------------------------------------
@GET_EX_K39(ARGS)
#FUNCTION
#LOCALSIZE 30
SPLIT ARGS, "/", LOCALS

LOCAL:0 = NOWEX:MASTER:Ｃ절정
LOCAL:1 = NOWEX:MASTER:Ｖ절정
LOCAL:2 = NOWEX:MASTER:Ａ절정
LOCAL:3 = NOWEX:MASTER:Ｂ절정
LOCAL:4 = NOWEX:MASTER:분유
LOCAL:5 = NOWEX:MASTER:사정
LOCAL:6 = NOWEX:MASTER:방뇨
LOCAL:7 = NOWEX:Ｃ절정
LOCAL:8 = NOWEX:Ｖ절정
LOCAL:9 = NOWEX:Ａ절정
LOCAL:10 = NOWEX:Ｂ절정
LOCAL:11 = NOWEX:분유
LOCAL:12 = NOWEX:사정
LOCAL:13 = NOWEX:MASTER:시오후키

;시오후키が追加당했다けど번호入れ替えるの面倒なんで取りあえずLOCAL:13で
SELECTCASE ARGS
	CASE "MC절정"
		RETURNF LOCAL:0
	CASE "MV절정"
		RETURNF LOCAL:1
	CASE "MA절정"
		RETURNF LOCAL:2
	CASE "MB절정"
		RETURNF LOCAL:3
	CASE "M분유"
		RETURNF LOCAL:4
	CASE "M사정"
		RETURNF LOCAL:5
	CASE "M드라이사정"
		RETURNF LOCAL:5 == 1
	CASE "M대량사정"
		RETURNF LOCAL:5 == 3
	CASE "M조루사정"
		RETURNF LOCAL:5 == 4
	CASE "MA責め사정"
		RETURNF LOCAL:5 == 5
	CASE "M통상사정"
		RETURNF LOCAL:5 == 2
	CASE "M방뇨"
		RETURNF LOCAL:6
	CASE "TC절정"
		RETURNF LOCAL:7
	CASE "TV절정"
		RETURNF LOCAL:8
	CASE "TA절정"
		RETURNF LOCAL:9
	CASE "TB절정"
		RETURNF LOCAL:10
	CASE "T분유"
		RETURNF LOCAL:11
	CASE "T사정"
		RETURNF LOCAL:12
	CASE "T드라이사정"
		RETURNF LOCAL:12 == 1
	CASE "T대량사정"
		RETURNF LOCAL:12 == 3
	CASE "T통상사정"
		RETURNF LOCAL:12 == 2
	CASE "M_CVAB절정"
		RETURNF LOCAL:0 > 0 && LOCAL:1 > 0 && LOCAL:2 > 0 && LOCAL:3 > 0
	CASE "M_CVA절정"
		RETURNF LOCAL:0 > 0 && LOCAL:1 > 0 && LOCAL:2 > 0
	CASE "M_CVB절정"
		RETURNF LOCAL:0 > 0 && LOCAL:1 > 0 && LOCAL:3 > 0
	CASE "M_CV절정"
		RETURNF LOCAL:0 > 0 && LOCAL:1 > 0
	CASE "M_CA절정"
		RETURNF LOCAL:0 > 0 && LOCAL:2 > 0
	CASE "M_CB절정"
		RETURNF LOCAL:0 > 0 && LOCAL:3 > 0
	CASE "T_CVAB절정"
		RETURNF LOCAL:7 > 0 && LOCAL:8 > 0 && LOCAL:9 > 0 && LOCAL:10 > 0
	CASE "T_CVA절정"
		RETURNF LOCAL:7 > 0 && LOCAL:8 > 0 && LOCAL:9 > 0
	CASE "T_CVB절정"
		RETURNF LOCAL:7 > 0 && LOCAL:8 > 0 && LOCAL:10 > 0
	CASE "T_CV절정"
		RETURNF LOCAL:7 > 0 && LOCAL:8 > 0
	CASE "T_CA절정"
		RETURNF LOCAL:7 > 0 && LOCAL:9 > 0
	CASE "T_CB절정"
		RETURNF LOCAL:7 > 0 && LOCAL:10 > 0
	CASEELSE
		CALLF PRINT_ERRORMESSAGE("@GET_EX_K39に無効な定義が渡されま했다")
		RETURNF -1
ENDSELECT
RETURNF 0

