;-------------------------------------------------
;関数名:KOJO_RAND_K39
;概　要:구상をランダムで生成する
;引　数:ARGS…行為の分類
;引　数:ARG…1でTARGET　2でMASTERの상태변화
;備　考:式中関数
;基本的に@FADE、若しくは@PRINTCOLORで使用する事を想定。文字色指定はそちらに丸投げ
;SQNクズノハ구상にインスパイアされて作った。作者のmaoの人には感謝を
;-------------------------------------------------
@KOJO_RAND_K39(ARGS, ARG)
#FUNCTIONS
LOCALS = 
FOR LOCAL, 0, ARG
	SIF LOCAL
		LOCALS += ""
IF ARG == 1 ;죠교자
	SELECTCASE ARGS
		CASE "키스", "乳首吸い"
			;죠교자の상태변화派生
			SELECTCASE GET_CONDITION(TARGET)
				CASE "정욕", "울굴"
					SELECTCASE RAND:25
						CASE 0
							LOCALS = %LOCALS%ちゅぅ%RANDTEXT_K39("っ、", "っ！　")%%WARD_C_K39()%ゅっ%ELLIPSIS2_K39()%ぷは、%WARD_A_K39()%む
						CASE 1
							LOCALS = %LOCALS%%WARD_A_K39()%むっ%ELLIPSIS2_K39()%%WARD_C_K39()%ぅゅっ…%RANDTEXT_K39("ちゅく", "ちゅぷ", "くちゅ")%、チュ
						CASE 2
							LOCALS = %LOCALS%%RANDTEXT_K39("ペロッ", "ぺろっ")%、ぴちゅ%ELLIPSIS2_K39()%%RANDTEXT_K39("ぢゅく", "ちゅく", "ちゅぷ")%、ちゅぅっ
						CASE 3
							LOCALS = %LOCALS%%RANDTEXT_K39("クチュ", "くちゅ", "ちゅく")%、じゅる%RANDTEXT_K39("っ", "ッ", "＿")%%ELLIPSIS2_K39()%ん、んく
						CASE 4
							LOCALS = %LOCALS%ちゅぅぅっ！ ぷは%RANDTEXT_K39("っ", "＿")%、ん%ELLIPSIS2_K39()%んく
						CASE 5
							LOCALS = %LOCALS%%WARD_A_K39()%むっ、%RANDTEXT_K39("クチュ", "くちゅ", "ちゅく")%…%WARD_C_K39()%ゅっ、ちゅぷ
						CASE 6
							LOCALS = %LOCALS%んくっ%ELLIPSIS2_K39()%ぁ…%WARD_A_K39()%むっ、んちゅぅ
						CASE 7
							LOCALS = %LOCALS%%RANDTEXT_K39("ん", "＿")%ん%RANDTEXT_K39("っ", "ッ")%……ふ%ELLIPSIS2_K39()%%RANDTEXT_K39("はぁ", "ハァ")%、%WARD_A_K39()%むっ…んく
						CASE 8
							LOCALS = %LOCALS%%RANDTEXT_K39("ふ", "ん")%%ELLIPSIS2_K39()%%RANDTEXT_K39("チュッ", "ちゅっ")%、ちゅく…、%WARD_A_K39()%む
						CASE 9
							LOCALS = %LOCALS%%WARD_A_K39()%っ、はぁ%ELLIPSIS2_K39()%んく、ちゅぅ%RANDTEXT_K39("っ", "＿")%…ちゅく
						CASE 10
							LOCALS = %LOCALS%%RANDTEXT_K39("はぁ、はぁ", "ハァ、ハァ")%%ELLIPSIS2_K39()%ぁ、んん%RANDTEXT_K39("っ", "＿")%…んく
						CASE 11
							LOCALS = %LOCALS%ふっ、ん%RANDTEXT_K39("ん", "く")%っ%ELLIPSIS2_K39()%ちゅ%RANDTEXT_K39("く", "ぷ")%、ぴちゅ
						CASE 12
							LOCALS = %LOCALS%ん%ELLIPSIS2_K39()%ぁ…、%WARD_A_K39()%むっ、んん%RANDTEXT_K39("っ", "＿")%…んく
						CASE 13
							LOCALS = %LOCALS%ぷは%RANDTEXT_K39("っ", "＿")%、はぁ%ELLIPSIS2_K39()%%WARD_A_K39()%むっ、%WARD_C_K39()%ゅぅぅっ！ ん
						CASE 14
							LOCALS = %LOCALS%ふぁ…はぁ%ELLIPSIS2_K39()%んく、んちゅ%RANDTEXT_K39("ぅ", "っ", "＿")%
						CASE 15
							LOCALS = %LOCALS%%RANDTEXT_K39("クチュ", "くちゅ", "ちゅく", "ぢゅく")%、%WARD_C_K39()%ゅぷ…、%RANDTEXT_K39("チュッ", "ちゅっ", "ちゅ")%、ん、んくっ
						CASE 16
							LOCALS = %LOCALS%チュッ、ちゅ%RANDTEXT_K39("ぅっ", "っ", "ぅぅっ")%%ELLIPSIS2_K39()%ぁ、%WARD_A_K39()%むっ、んふ
						CASE 17
							LOCALS = %LOCALS%んぅっ、んく%RANDTEXT_K39("っ", "＿")%…%RANDTEXT_K39("はぁ、はぁ", "ハァ、ハァ")%%ELLIPSIS2_K39()%ちゅ%RANDTEXT_K39("ぅ", "ぅぅ", "っ")%
						CASE 18
							LOCALS = %LOCALS%%RANDTEXT_K39("ぴ", "く", "ん")%ちゅ、ちゅ%RANDTEXT_K39("く", "ぷ", "っ")%%ELLIPSIS2_K39()%ん、んちゅ%RANDTEXT_K39("ぅっ", "っ", "＿")%…んく、んく
						CASE 19
							LOCALS = %LOCALS%ふぅ、んっ…んく、%RANDTEXT_K39("んく", "＿")%%RANDTEXT_K39("っ", "＿")%%ELLIPSIS2_K39()%%WARD_C_K39()%ゅぅっ、ちゅく
						CASE 20
							LOCALS = %LOCALS%%WARD_A_K39()%むっ、ん%ELLIPSIS2_K39()%んく、んちゅ…っ、ちゅく
						CASE 21
							LOCALS = %LOCALS%じゅっ、%WARD_C_K39()%ゅぅっ%ELLIPSIS2_K39()%%RANDTEXT_K39("ちゅく", "ちゅぷ")%…ぴちゅ
						CASE 22
							LOCALS = %LOCALS%クチュ、%RANDTEXT_K39("くちゅっ", "ちゅぷ", "ちゅく")%%ELLIPSIS2_K39()%ぁ…%WARD_A_K39()%むっ、んく
						CASE 23
							LOCALS = %LOCALS%ちゅ%RANDTEXT_K39("ぅっ、", "ぅぅっっ！")% ぷは、%RANDTEXT_K39("はぁ", "ハァ")%……%WARD_A_K39()%むっ、んく
						CASE 24
							LOCALS = %LOCALS%ちゅ%RANDTEXT_K39("＿", "く", "ぷ", "っ")%、ちゅくっ…%RANDTEXT_K39("ペロ", "ぺろ")%%RANDTEXT_K39("ッ", "っ")%、ん%ELLIPSIS2_K39()%ちゅぅ
					ENDSELECT
				CASEELSE ;その他
					SELECTCASE RAND:25
						CASE 0
							LOCALS = %LOCALS%んぅっ、ん%RANDTEXT_K39("っ", "ッ", "＿")%%ELLIPSIS2_K39()%んく
						CASE 1
							LOCALS = %LOCALS%%WARD_A_K39()%むっ、んっ%ELLIPSIS2_K39()%ちゅ%RANDTEXT_K39("ぷ", "く", "＿")%
						CASE 2
							LOCALS = %LOCALS%ちゅっ、んちゅ%ELLIPSIS2_K39()%ん%RANDTEXT_K39("ん", "く", "＿")%
