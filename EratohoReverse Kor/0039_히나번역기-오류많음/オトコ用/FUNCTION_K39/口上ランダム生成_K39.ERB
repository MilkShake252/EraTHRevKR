							LOCALS = %LOCALS%%RANDTEXT_K39("ちゅぅ", "ちゅく", "ちゅ")%っ…くちゅっ%ELLIPSIS2_K39()%ぢゅ
						CASE 6
							LOCALS = %LOCALS%ちゅ%ELLIPSIS2_K39()%%WARD_A_K39()%むっ…んんっ
						CASE 7
							LOCALS = %LOCALS%%RANDTEXT_K39("ちゅっ", "チュっ")%、%RANDTEXT_K39("ちゅう", "ちゅく", "ちゅる")%…っ…ん
						CASE 8
							LOCALS = %LOCALS%ちゅく、%RANDTEXT_K39("ちゅく", "ちゅぷ", "クチュ", "くちゅ")%…ちゅる
						CASE 9
							LOCALS = %LOCALS%%WARD_A_K39()%むっ…ん%RANDTEXT_K39("、", "っ、", "ッ", "……", "……、")%ちゅっ、%RANDTEXT_K39("ちゅぷ", "ちゅく", "チュ")%
						CASE 10
							LOCALS = %LOCALS%%RANDTEXT_K39("あ", "ぁ")%むっ…%RANDTEXT_K39("ちゅ", "ちゅく", "ちゅぷ")%っ、チュッ
						CASE 11
							LOCALS = %LOCALS%%RANDTEXT_K39("は", "あ", "ぁ", "ん")%む%RANDTEXT_K39("っ、", "……、")%んふっ……ちゅ
						CASE 12
							LOCALS = %LOCALS%ちゅっ、っちゅ…%RANDTEXT_K39("くちゅ", "ちゅぷ", "ちゅぅ")%
						CASE 13
							LOCALS = %LOCALS%んふ、ちゅ%RANDTEXT_K39("っ、", "……", "……、")%ちゅ%RANDTEXT_K39("ぅ", "ぅぅ", "＿")%
						CASE 14
							LOCALS = %LOCALS%%WARD_A_K39()%む%RANDTEXT_K39("っ", "＿")%…れろ%RANDTEXT_K39("っ", "＿")%……%RANDTEXT_K39("ん", "く", "＿")%ちゅ
						CASE 15
							LOCALS = %LOCALS%ちゅ%RANDTEXT_K39("っ、", "ぅっ、", "……")%ん%RANDTEXT_K39("っ、", "、")%んく
						CASE 16
							LOCALS = %LOCALS%ん……んく%ELLIPSIS2_K39()%%RANDTEXT_K39("チュ", "ちゅ", "ちゅく", "ちゅぷ")%
						CASE 17
							LOCALS = %LOCALS%ん%RANDTEXT_K39("ぅ", "っ", "ッ")%……ふ%RANDTEXT_K39("＿", "…", "……、")%んく
						CASE 18
							LOCALS = %LOCALS%%RANDTEXT_K39("チュッ", "チュ", "ちゅ", "ちゅっ")%、%RANDTEXT_K39("ちゅく", "ちゅる")%%ELLIPSIS2_K39()%ちゅぷ
						CASE 19
							LOCALS = %LOCALS%くちゅ、%RANDTEXT_K39("ちゅ", "ちゅく", "ちゅぷ", "チュ")%%ELLIPSIS2_K39()%ん%RANDTEXT_K39("、", "っ、", "ッ、")%んく
						CASE 20
							LOCALS = %LOCALS%っはぁ、%RANDTEXT_K39("ペロッ", "ぺろ", "ぺろっ")%%ELLIPSIS2_K39()%ちゅ%RANDTEXT_K39("っ", "ぅっ", "ッ", "ぅぅ")%
						CASE 21
							LOCALS = %LOCALS%%RANDTEXT_K39("んっ", "ん", "んッ")%%RANDTEXT_K39("……", "…", "…っ、")%%RANDTEXT_K39("はっ、はぁ", "ハァ、ハァ")%……ちゅ%RANDTEXT_K39("ぅ", "うぅ", "ぅぅ")%
						CASE 22
							LOCALS = %LOCALS%んく、%RANDTEXT_K39("んっ…", "ん……")%%RANDTEXT_K39("チュッ、", "ちゅく", "ちゅぷ")%くちゅ
						CASE 23
							LOCALS = %LOCALS%ちゅく、%RANDTEXT_K39("ちゅぷ", "ちゅ", "くちゅ", "んっ")%%RANDTEXT_K39("…", "……", "っ、", "…っ、")%ちゅぅぅ
