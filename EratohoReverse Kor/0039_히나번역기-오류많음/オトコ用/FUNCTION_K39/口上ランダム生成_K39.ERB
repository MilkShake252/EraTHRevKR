						CASE 14
							LOCALS = %LOCALS%%RANDTEXT_K39("ぺろっ", "ペロッ")%、ぴち%RANDTEXT_K39("ゃ", "ゅ")%%ELLIPSIS2_K39()%ちゅ%RANDTEXT_K39("ぅ", "ぅぅ", "＿")%っ、くちゅ
						CASE 15
							LOCALS = %LOCALS%%RANDTEXT_K39("レロ", "れろ")%%RANDTEXT_K39("っ", "＿")%、ちゅ%RANDTEXT_K39("っ", "＿")%、つぷ%ELLIPSIS2_K39()%ん、ぢゅぅ
						CASE 16
							LOCALS = %LOCALS%%RANDTEXT_K39("ペロッ", "ぺろっ")%、くちゅ%RANDTEXT_K39("っ", "＿")%%ELLIPSIS2_K39()%ちゅ%WARD_B_K39()%、ちゅぅ
