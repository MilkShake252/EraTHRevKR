							LOCALS = %LOCALS%%WARD_A_K39()%むっ、%RANDTEXT_K39("んく", "んちゅ", "ちゅ")%%RANDTEXT_K39("…", "……", "っ、", "…っ、")%んっ
					ENDSELECT
