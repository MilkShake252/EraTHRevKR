;-------------------------------------------------
;関数名:COLOR_K39
;概　要:@RGBCOLORに代わり、文字列による色指定を行う
;引　数:ARGS…指定する色名、または当該文字色のキャラ名
;戻り値:RGBCOLOR指定の値
;備　考:式中関数
;使用例:CALL PRINTCOLORW(COLOR2_K39("white"), "『この文字列は白で表示され改行待ちする』")
;-------------------------------------------------
@COLOR_K39(ARGS)
#FUNCTION
SELECTCASE ARGS
	;HEART用。죠교자の만족度に応じて色を5%刻みで변화시킨다
	CASE "ピンク", "pink"
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				RETURNF RGBCOLOR(255, 179, 222)
			CASE "一般"
				RETURNF RGBCOLOR(255, 191, 235)
			CASE "低い"
				RETURNF RGBCOLOR(255, 204, 247)
		ENDSELECT
	CASE "히나", "HINA", "red", "赤"
		RETURNF RGBCOLOR(181, 33, 59)
	CASE "白", "white"
		RETURNF RGBCOLOR(192, 192, 192)
	CASE "黒", "black"
		RETURNF RGBCOLOR(0, 0, 0)
	CASE "유카리", "YUKARI"
		RETURNF RGBCOLOR(204, 153, 255)
	CASE "엘리스", "ALICE"
		RETURNF RGBCOLOR(54, 166, 255)
	CASE "사쿠야", "SAKUYA"
		RETURNF RGBCOLOR(172, 175, 210)
	CASE "上海", "test"
		RETURNF RGBCOLOR(150,200,150)
	CASEELSE
		CALLF PRINT_ERRORMESSAGE("@COLOR_K39に無効な定義(%ARGS%)が渡されま했다")
		RETURNF -1
ENDSELECT

@COLOR2_K39(ARGS)
#FUNCTION
SELECTCASE ARGS
	CASE "엘리스", "ALICE"
		RETURNF 0x36A6FF
	CASE "사쿠야", "SAKUYA"
		RETURNF 0xACAFD2
	CASE "赤", "red", "히나", "HINA", "RED"
		RETURNF 0xA8142F
	CASE "白", "white", "WHITE"
		RETURNF 0xFFFFFF
	CASE "ピンク", "pink", "PINK"
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				RETURNF 0xFFB3DE
			CASE "一般"
				RETURNF 0xFFBFBE
			CASE "低い"
				RETURNF 0xFFCCF7
		ENDSELECT
	CASE "黒", "black", "BLACK"
		RETURNF 0x000000
	CASE "レミィ", "パチェ", "orchild", "ORCHILD"
		RETURNF 0xDA70D6
	CASE "上海", "test"
		RETURNF 0x96C896
	CASE "유카리", "YUKARI"
		RETURNF 0xCC99FF
	CASEELSE
		CALLF PRINT_ERRORMESSAGE("@COLOR2_K39に無効な定義(%ARGS%)が渡されま했다")
		RETURNF -1
ENDSELECT

@COLOR3_K39, ARGS, ARG = -1
#FUNCTION
#LOCALSIZE 1
#DIM PREV
IF ARGS == "PREV"
	RETURNF PREV
ELSE
	PREV = GETCOLOR()
ENDIF
SELECTCASE TOUPPER(ARGS)
	CASE "RESET", "DEFAULT", "DEF"
		LOCAL = GETDEFCOLOR()
	CASE "BG"
		LOCAL = GETBGCOLOR()
	CASE "DEFBG"
		LOCAL = GETDEFBGCOLOR()
	CASE "ALICE"
		LOCAL = 0x36A6FF
	CASE "SAKUYA"
		LOCAL = 0xACAFD2
	CASE "RED", "HINA"
		LOCAL = 0xA8142F
	CASE "WHITE"
		LOCAL = 0xFFFFFF
	CASE "PINK"
		LOCAL = 0xFFB3DE
	CASE "BLACK"
		LOCAL = 0x000000
	CASE "ORCHILD"
		LOCAL = 0xDA70D6
	CASE "GOLD"
		LOCAL = 0x96C896
	CASE "YUKARI"
		LOCAL = 0xCC99FF
	CASEELSE
		IF ISNUMERIC(ARGS)
			LOCAL = TOINT(ARGS)
		ELSE
			THROW @COLOR3_K39に등록されていない名称[%ARGS%]が指定されま했다。
		ENDIF
ENDSELECT
SIF ARG >= 0
	LOCAL = BRIGHTCOLORF(ARG, LOCAL)
RETURNF LOCAL

