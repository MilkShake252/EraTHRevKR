					}
				CASE 1
					{
					CALL HTMLPRINTW(@"「하앗, 아아읏%AHEART% 이,가게해줘%AHEART% " + 
;OriginalString : おまんこでっ、가게해줘ぇっっ…ッ…！」")
					@"보지로,가게해줘엣……！」")
					}
				CASE 2
					{
					CALL HTMLPRINTW(@"「응%VOICEDDOT%읏%AHEART% 쿠웃%AHEART% " + 
