IF IS_NOWACTNAME("성기마찰한다/파이즈리한다")
	CALL QUAKE(4)
	IF GET_KFLAG_K39("상사상애")
		SELECTCASE RANDCOM(2, 20)
			CASE 0
				{
				CALL HTMLPRINTW(@"「%RAND_SPLIT("후/아/응")%\@ 
				PARAM > 8499 ? ぁ # 
