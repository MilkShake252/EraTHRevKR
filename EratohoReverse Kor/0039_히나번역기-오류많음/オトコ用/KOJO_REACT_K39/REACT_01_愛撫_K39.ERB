						CALL INPUTINTD(0, 1, 2,)
						SELECTCASE RESULT
							CASE 0
								PRINTFORMW 「그렇게 까지 말해지면 어쩔 수 없네요」
								{
								CALL HTMLPRINTW(@"「이봐요, 째를 찔러 준다.\@ 
								PANTSCHECK(1) ? 、パンツの中に # \@思いっきりぶちまけて%HTML_PS_K39()%」")
