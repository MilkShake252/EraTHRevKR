				CASE 1
					PRINTFORMW 「에？ 이런 건이 부끄러운거야？」
					CALL HTMLPRINTW(@"「언제나 좀 더 부끄러운 일 하고 있는데」")
			ENDSELECT
		;その他
		ELSE
			SELECTCASE RANDREACT(3, 103)
				CASE 0
					CALL HTMLPRINTW(@"「아라, 부끄럽습니까？ 사랑스러운 곳도 있군요%HTML_PS_K39()%」")
				CASE 1
					PRINTFORMW 「코코를 나에게 만지작거려지는 것은 부끄럽습니까？」
					CALL HTMLPRINTW(@"「그렇지만, 이런 것은 끝나지 않기 때문에%HTML_PS_K39()%」")
				CASE 2
					CALL HTMLPRINTW(@"「그렇게 얼굴을 붉게 해 버려, 사랑스럽네요%HTML_PS_K39()%」")
					CALL FADE_TO_CLEARLINE(15, "「クスクス…」")
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
