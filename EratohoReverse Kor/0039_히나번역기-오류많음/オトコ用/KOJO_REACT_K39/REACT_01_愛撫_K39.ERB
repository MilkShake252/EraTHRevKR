SYMBOL    '= HTML_PS_K39()
STRING    '= RAND_SPLIT("ふふ/うふ/ふふっ/うふっ/くすっ/クスクス") + "…"
MSTR      '= CNAME(MASTER, TARGET)

;밀어넘어짐終了なら1を返して終わり
SIF TFLAG:ACT파생
	RETURN 1

;REACT27, ACT파생0:밀어 넘어트린다
;==========================
IF !TFLAG:ACT파생
	;REACT분류が受け入れる
	;---------------------
	IF IS_COMGRONAME("受け入れる")
		;REACT부하による派生
		SELECTCASE GET_COM_RANK()
			CASE "自然な행동", "無理のない행동"
				{
				PRINTFORMW 「밀어 넘어뜨려져 기쁜 듯한 얼굴 하고 있다\@ FAV_RATING 
;OriginalString : 남자って、ちょっとどうなの？ って気もするけど 
				? 남자읏, 조금 어때？ 라는 생각도 들지만 
;OriginalString : 男の人ってどうなんでしょう？ という気もしますけど \@…」
				# 남자는 어떻습니까？ 그렇다고 할 생각도 듭니다만 \@…」
				}
				CALL HTMLPRINTW(@"「이것은 이것대로 나쁘지 않다\@ FAV_RATING ? 원이군요 # (이)군요 \@%SYMBOL%」")
				CALL FADE_TO_CLEARLINE(15, @"「%STRING%」")
			CASE "無理を했다행동"
				{
				PRINTFORML 「그렇게 자주, 그래서 좋다\@ FAV_RATING 
;OriginalString : のよ # んですよ 
				? 의 # 응(이에)예요 
				\@。%MSTR%だって満更でもない\@ FAV_RATING 
;OriginalString : のよね # んですよね \@？」
				? 의군요 # 응(이)군요 \@？」
				}
				PRINTFORMW 「지금부터 나로 되는 것에 기대하고 있다\@ FAV_RATING ? 의군요 # 응(이)군요 \@？」
				CALL FADE_TO_CLEARLINE(15, @"「%STRING%」")
				CALL HTMLPRINTW(@"「%MSTR%의 그 표정을 보면 판단\@ FAV_RATING ? 원 # 야 \@…%SYMBOL%」")
		ENDSELECT
		RETURN 1
	ENDIF

	;//消費기력の派生を設けたいが今は見送る
	;REACT분류が적극적으로 한다・태연하게 한다
	;---------------------------------
	IF IS_COMGRONAME("적극적으로 한다/태연하게 한다")
		;호의호감도で派生
		IF FAV_RATING
			PRINTFORMW 「상당히 얌전해져 버려서…」
			{
			PRINTFORMW 「%RAND_SPLIT(
;OriginalString : それって、あとは好きにしてって事？/" + 
			"그 거, 뒤는 마음대로 해라는 일？/" + 
;OriginalString : じゃ、後は好きなようにさせてもらうわ。ふふ…/" + 
			"그러면, 뒤는 좋을대로 시켜 받아요. 후후…/" + 
;OriginalString : ふふっ…、いいわ。そのままじっとしてて……")%」
			"후훗…, 좋아요. 그대로 가만히 하고 있어……")%」
			}
		ELSE
			PRINTFORMW 「어머나, 얌전하네요？」
			CALL HTMLPRINTW(@"「그럼, 뒤는 내의 좋아하게 시켜 받아요%SYMBOL%」")
		ENDIF
		RETURN 1
	ENDIF

	;REACT분류が용서를 빌다
	;---------------------
	IF IS_COMGRONAME("용서를 빌다")
		IF FAV_RATING
			PRINTFORMW 「이봐요, 얌전하게 해…」
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT(
;OriginalString : 可愛がってあげる/" + 
			"귀여워해 준다/" + 
;OriginalString : 悪いようにはしないわ/" + 
			"나쁘게는 하지 않아요/" + 
;OriginalString : 動かれるとその…、色々とやりにくいし")%…%SYMBOL%」")
			"움직여지면 그…, 다양하게 하기 어렵고")%…%SYMBOL%」")
			}
		ELSE
			CALL HTMLPRINTW(@"「%RAND_SPLIT("자, 이제 놓치지 않아요/얌전하게 하고 있어 주세요？ 후후…")%%SYMBOL%」")
		ENDIF
		RETURN 1
	ENDIF

	;REACT분류が소극적으로 한다
	;---------------------
	IF IS_COMGRONAME("소극적으로 한다")
		;COM10, 부끄러워한다
		IF IS_NOWCOMNAME("부끄러워한다")
			IF FAV_RATING
				PRINTFORMW 「 나에게 밀어 넘어뜨려지는 것이 그렇게 부끄러운거야？」
				CALL HTMLPRINTW(@"「후후, 사랑스럽다.키스했다구 되어 버린다…%SYMBOL%」")
				CALL CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K39(, , "키스", 1)
				;키스を捏造
				CALL SET_KISS
			ELSE
				CALL HTMLPRINTW(@"「아라, 이런 일로부끄러워한다…사랑스럽네요%SYMBOL%」")
			ENDIF
			RETURN 1
		ENDIF

		;その他のCOMの場合
		IF FAV_RATING
			PRINTFORML 「그것, 조금 소극너무 적인 있고？」
			PRINTFORMW 「나머지는 나에게 맡겨라는 일이야？」
			CALL HTMLPRINTW(@"「아무튼…, 그렇다면 그걸로. 나는 별로 좋지만%SYMBOL%」")
		ELSE
			PRINTFORML 「그다지 내켜하는 마음이 아닌 것 같지만…」
			PRINTFORMW 「라고 해서 저항할 생각도 없네요？」
			CALL HTMLPRINTW(@"「우후%HTML_AH2_K39()% 그럼, 그런 일이라면……」")
		ENDIF
		RETURN 1
	ENDIF
ENDIF
;REACTで必ず完結시킨다
RETURN 1
[SKIPSTART]
밀어넘어짐は애무系の中でもちょっと特殊なので汎用リアクションだと辻褄が合わない可能性が高い
そして汎用リアクションに於いてACT밀어넘어짐用の分岐を漏れなく網羅するのは現実的はない
故にこの箇所で完結させてしまう
[SKIPEND]


