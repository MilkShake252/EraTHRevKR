;히나구상 회화系個別リアクション구상ファイル

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;


;#################
;회화系 REACT0～9
;#################
[SKIPSTART]
	#TFLAG:ACT
		0=지금 기분을 묻다
		1=자신에게의 마음을 묻다
		2=성에 관한 이야기를 하다
		3=상냥하게 위로한다
		4=엄하게 위협한다
		5=의상 변경을 요구한다
		6=합의를 요구한다
		7=징계한다고 위협한다
		8=고함치다
		9=기분나쁘게 웃다

회화系は汎用処理には向かないので、汎用リアクション(COM)に流さず個別リアクション(REACT)
で完結させてしまうのが望ましいと考えます
[SKIPEND]

;///////////////////////
;REACT0, 지금 기분을 묻다
;///////////////////////
[SKIPSTART]
・ACT파생への代入は죠교대상のリアクションに応じて@ACTION_APPLY2_0にて行われる

・TFLAG:ACT파생
  ├0=歯切れ悪く答えた
  ├1=勇気を出して強く当たった
  ├2=皮肉
  ├3=はっきりと今の気持ちを告げた
  ├4=まあまあ悪くはない
  └5=気持ち良いこと했다い)
[SKIPEND]
@KOJO_MALE_REACT_K39_0
#DIM DYNAMIC FAV_RATING
FAV_RATING = GET_KFLAG_K39("상사상애")

;負荷2ならREACTを飛ばす
SIF GET_COM_RANK() == "無理を했다행동"
	RETURN 0

;ACT파생,0 歯切れ悪く質問に答えた // REACT분류が소극적으로 한다で、リアクションが빈정거린다以外の場合
;================================
IF !TFLAG:ACT파생
	;볼개그装備中
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;その他
	PRINTFORMW 「\@ FAV_RATING ? 저기, 뭔가 말했다？ # 미안해요, 자주(잘) 들리지 않았던 것입니다만… \@」
	RETURN 1
ENDIF

;ACT파생,1 勇気を出して強く当たった // リアクションが강하게 응답한다の場合。REACT분류は"거부"にセットされる
;==================================
IF TFLAG:ACT파생 == 1
	;볼개그装備中
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;その他
	PRINTFORMW 「\@ FAV_RATING ? 상당히, 위세가 좋은거네 # 건강은 좋은 것 같네요 \@…」
	RETURN 1
ENDIF

;ACT파생,2 皮肉「今の気分は、と聞かれても」 // リアクションが빈정거린다の場合。REACT분류は"거부"にセットされる
;==========================================
IF TFLAG:ACT파생 == 2
	;볼개그装備中
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;その他
	IF FAV_RATING
		PRINTFORML 「뭐그것, 야유？」
		PRINTFORMW 「그러한 것은 좋아하지 않지만…」
	ELSE
		PRINTFORML 「야유, 입니까…？」
		PRINTFORMW 「너무 그러한 것은, 좋아하지 않습니다만…」
	ENDIF
	RETURN 1
ENDIF

;ACT파생,3 はっきりと今の気持ちを告げた // REACT분류が적극적으로 한다の場合
;=======================================
IF TFLAG:ACT파생 == 3
	;初回
	IF FIRSTREACT(30)
		{
		CALL HTMLPRINTW(@"「\@ FAV_RATING 
;OriginalString : ふふ、愉しんでもらえてるみたいね 
		? 후후, 즐겨 받아지고 있는 것 같구나 
;OriginalString : そうですか。愉しんでもらえてるようで何よりです \@%HTML_PS_K39()%」")
		# 그렇습니까. 즐겨 받아지고 있는 것 같고 최상입니다 \@%HTML_PS_K39()%」")
		}
		RETURN 1
	ENDIF

	;상사상애
	IF FAV_RATING
		;역강간で질내사정의後
		IF TFLAG:전회질싸 == 2
			PRINTFORML 「아무튼, 당연하지만！」
			PRINTFORMW 「 나의 질내에 이렇게 방편 물어라고, 자주(잘) 없었다니 말할 수  것 않아요」
		;통상
		ELSE
			PRINTFORMW 「언제나 그 정도 솔직하다면, 나도 다양하게 편하지만？」
		ENDIF
		;フェードアウト
		CALL FADE_TO_CLEARLINE(20, @"「%RAND_SPLIT("ふふっ/くすくす/くすっ")%…」")
		RETURN 1
	ENDIF

	;죠교대상からの호의1000以上
	IF AFFECTION("죠교済み", TARGET)
		;역강간で질내사정의後　と　その他
		{
		CALL HTMLPRINTW(@"「\@ TFLAG:전회질싸 == 2 
;OriginalString : 私の질내がそんなに良かったですか…？ 
		? 나의질내하지만 그렇게 좋았던 것입니까…？ 
;OriginalString : うふ…、私も%CNAME(MASTER, TARGET)%の事好きですよ%HTML_PS_K39()% \@」")
		# 우후…, 나도%CNAME(MASTER, TARGET)%의 일을 좋아해요%HTML_PS_K39()% \@」")
		}
		RETURN 1
	ENDIF

	;죠교대상からの호의500以上
	IF AFFECTION("目覚め")
		;역강간で질내사정의後
		IF TFLAG:전회질싸 == 2
			PRINTFORML 「상태가 좋았다？ %RAND_SPLIT("추잡합니다/천한 말투입니다")%그런데…」
			CALL HTMLPRINTW(@"「그렇지만, 후후. 즐겨 받을 수 있던 것 같고 최상입니다…%HTML_PS_K39()%」")
		;통상
		ELSE
			CALL HTMLPRINTW(@"「대단히솔직(이)가 되어 져했다저기. 후후」")
		ENDIF
		RETURN 1
	ENDIF

	;その他
	CALL HTMLPRINTW(@"「\@ FAV_RATING ? 우후, 사랑스럽다 # 솔직그리고 좋아요 \@%HTML_PS_K39()%」")
	RETURN 1
ENDIF

;ACT파생,4 まあまあ悪くはない // REACT분류が태연하게 한다の場合
;=============================
IF TFLAG:ACT파생 == 4
	IF TEQUIP:성교봉사중
		{
		PRINTFORMW 「\@ FAV_RATING 
;OriginalString : まあまあって何よそれ…、なんかムカつくんですけどー！？ 
		? 그저라는건 뭐야 그것…, 뭔가 화납니다만―！？ 
;OriginalString : それは…솔직には喜べないですね。流石に、今の状況では… \@」
		# 그것은…솔직에는 기뻐할 수 없네요. 과연, 지금의 상황에서는… \@」
		}
		RETURN 1
	ENDIF

	;その他
	{
	PRINTFORMW 「\@ FAV_RATING 
;OriginalString : ちょっと引っかかるわね。その言い方… 
	? 조금 걸려요. 그 말투… 
;OriginalString : それは何よりです。ちょっと釈然としませんけど… \@」
	# 그것은 최상입니다. 조금 석연으로 하지 않지만… \@」
	}
	RETURN 1
ENDIF

;ACT파생,5 気持ち良いこと했다い // ;REACT분류が受け入れるの場合
;===============================
;IF TFLAG:ACT파생 == 5
;ENDIF

;COM0, 침묵한다 // 지금 기분을 묻다に於けるCOM"침묵한다"は(質問に答えない)扱い
;===============
IF IS_NOWCOMNAME("침묵한다")
	;볼개그装備中
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;その他
	IF FAV_RATING
		PRINTFORMW 「응…, 어째서 입다물어？」
	ELSE
		PRINTFORML 「아무것도 말하고 싶지 않습니까？」
		PRINTFORMW 「 나, 그러면 곤란해 버립니다…」
	ENDIF
	RETURN 1
ENDIF

;COM7, 용서를 빌다
;================
IF IS_NOWCOMNAME("용서를 빌다")
	;「意見を出すなんていい度가슴ね！」징계・포인트+
	IF TFLAG:REACT파생 == 1
		{
		CALL HTMLPRINTW(@"「\@ FAV_RATING 
;OriginalString : %RAND_SPLIT("う/ふ")%ふ、だ～め%HTML_PS_K39()% 
		? %RAND_SPLIT("우/후")%ふ、だ～め%HTML_PS_K39()% 
;OriginalString : ごめんなさい。今更、遅いです… \@」")
		# 미안해요. 이제 와서, 늦습니다… \@」")
		}
		RETURN 1
	ENDIF

	;볼개그装備中
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;その他
	{
	CALL HTMLPRINTW(@"「\@ FAV_RATING 
;OriginalString : %RAND_SPLIT("う/ふ")%ふ、どうしようか%RAND_SPLIT("な/かしら")%%HTML_PS_K39()% 
	? %RAND_SPLIT("우/후")%ふ、どうしようか%RAND_SPLIT("な/かしら")%%HTML_PS_K39()% 
;OriginalString : もう！ いつも私が許すと思われても困ります… \@」")
	# 이제(벌써)！ 언제나 내가 허락한다고 생각되어도 곤란합니다… \@」")
	}
	RETURN 1
ENDIF

;COM33, 무서워한다
;==============
IF IS_NOWCOMNAME("무서워한다")
	;派生1なら汎用リアクションへ
	SIF TFLAG:REACT파생 == 1
		RETURN 0

	;볼개그装備中
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;その他
	IF FAV_RATING
		PRINTFORML 「응…, 어째서 무서워해？」
		PRINTFORMW 「그러한 것 그만두어%RAND_SPLIT("갖고 싶지만/")%…」
	ELSE
		PRINTFORMW 「무서워하게 해 버린 것 같네요…」
	ENDIF
	RETURN 1
ENDIF

;COM14, 쾌감을 참는다
;======================
IF IS_NOWCOMNAME("쾌감을 참는다")
	;볼개그装備中
	IF TEQUIP:볼개그
		IF FAV_RATING
			PRINTFORML 「몸가크가크 시켜, 필사적으로 참고 있는 것 같지만…」
			PRINTFORMW 「무엇을 그렇게 참아…？」
			CALL HTMLPRINTW(@"「…라는 지금 들어도, 조금 무리같구나…%HTML_PS_K39()%」")
		ELSE
			PRINTFORMW 「무엇인가 필사적으로 견디고 있는 것 같습니다만, 도대체 왜지요？」
			CALL HTMLPRINTW(@"「아, 대답하지 않아도 괜찮아요. 어차피 도취되어 듣지 않기 때문에. 우훗%HTML_PS_K39()%」")
		ENDIF
		RETURN 1
	ENDIF

	;オナホ장착中
	IF TEQUIP:오나홀
		IF FAV_RATING
			PRINTFORMW 「필사적으로 참고 있는 것 같지만, 지금 내가 시고 있으면 어떻게 되어버릴까 해들？」
			PRINTFORMW 「응, 어떻게야？ 좀 더 분명히 말하며」
		ELSE
			PRINTFORMW 「이를 악물지 않으면 이는 버릴 것 같습니까？」
			PRINTFORMW 「내가 움직여 봐도 괜찮아요？ 킥킥…」
		ENDIF
		RETURN 1
	ENDIF

	;それ以外
	IF FAV_RATING
		PRINTFORMW 「참지 않으면 이있을 것 같아？」
		CALL HTMLPRINTW(@"「후훗, 사랑스럽네요%HTML_PS_K39()%」")
	ELSE
		PRINTFORMW 「참지 않으면 이는 버릴 것 같습니까？」
	ENDIF
	RETURN 1
ENDIF

;COM15, 쾌감을 받아들인다
;========================
IF IS_NOWCOMNAME("쾌감을 받아들인다")
	;볼개그装備中
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;オナホ장착中
	IF TEQUIP:오나홀
		IF FAV_RATING
			PRINTFORML 「즐겨 받을 수 있고 있는 것 같구나？」
			PRINTFORMW 「그렇게 키모치 좋으면, 이대로 소레안에 내버리면？」
		ELSE
			PRINTFORMW 「우대신, 기분 좋을 것 같고 최상이네요♪」
			PRINTFORMW 「내가 시고 있어 좀 더 자주(잘) 해 줄까요？」
		ENDIF
		;フェードアウト
		CALL FADE_TO_CLEARLINE(20, @"「%RAND_SPLIT("ふふっ/くすっ")%…」")
		RETURN 1
	ENDIF

	;その他
	{
	CALL HTMLPRINTW(@"「\@ FAV_RATING 
;OriginalString : 솔직で可愛い%HTML_AH2_K39()%　もう키스했다くなっちゃう%RAND_SPLIT("わ/")% 
	? 솔직그리고 사랑스럽다%HTML_AH2_K39()% 이제(벌써)키스했다구 되어 버린다%RAND_SPLIT("와/")% 
;OriginalString : 솔직に感じてくれると、私も嬉しいです%HTML_PS_K39()% \@」")
	# 솔직에 느껴 주면, 나도 기쁩니다%HTML_PS_K39()% \@」")
	}
	RETURN 1
ENDIF


;////////////////////////////
;REACT1, 자신에게의 마음을 묻다
;////////////////////////////
[SKIPSTART]

・ACT파생への代入は죠교대상のリアクションに応じて@ACTION_APPLY2_1にて行われる

・TFLAG:ACT파생(0=嫌いじゃない…かも知れない…
  ├1=そんなことあるわけない！
  ├2=困るねぇ…好きじゃないといったら怒る？
  ├3=もう酷いことしなければ、もっと好きになれるかもしれない
  ├4=はっきりと好きと告げた
  └5=もっとすごいことしてくれば、もっと好きになれるかも)

[SKIPEND]
@KOJO_MALE_REACT_K39_1
#DIM DYNAMIC FAV_RATING
FAV_RATING = GET_KFLAG_K39("상사상애")

;負荷2ならREACTを飛ばす
SIF GET_COM_RANK() == "無理を했다행동"
	RETURN 0

;REACT1_0
;「嫌いじゃない…かも知れない…」 // REACT분류が소극적으로 한다の場合
;================================
IF !TFLAG:ACT파생
	;볼개그装備中
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;その他
	IF FAV_RATING
		PRINTFORMW 「네, 이제 와서 그것은 없지요！？ 조금 심하지 않다…？」
	ELSE
		PRINTFORML 「그렇게, 입니까…」
		PRINTFORMW 「일단 미움받지 않다는 것으로, 기뻐하고 있는입니까…」
	ENDIF
	RETURN 1
ENDIF

;REACT1_1
;「そんなことあるわけない！」 // リアクションが강하게 응답한다の場合
;============================
;REACT분류は"거부"にセットされる
IF TFLAG:ACT파생 == 1
	;볼개그装備中
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;その他
	IF 0
	ELSEIF FAV_RATING
		PRINTFORML 「은？ 도대체 무슨 말을 해！？」
		PRINTFORMW 「의미를 모르지만？」
	ELSEIF AFFECTION("익애", (FLAG:주인님)) && TALENT:TARGET:주인님
		PRINTFORMW 「액신이라고 해도 한사람의 여자입니다. 너무 심한 일, 말하지 마……」
		PRINTFORMDW %CALLNAME:TARGET%(은)는 몸을 진동시켜 울 것 같은 얼굴로 숙여 버렸다…
	ELSE
		PRINTFORMW 「그렇게, 입니까…」
	ENDIF
	RETURN 1
ENDIF

;REACT1_2,
;皮肉「困るねぇ…好きじゃないといったら怒る？」 // リアクションが빈정거린다の場合
;==============================================
IF TFLAG:ACT파생 == 2
	;죠교대상에의 호감도3000以上
	IF AFFECTION("盲愛", (FLAG:주인님)) && TALENT:TARGET:주인님 && !FAV_RATING
		PRINTFORMW 「, 그런！ 짓궂다니, 심하다…」
		PRINTFORMW 「 나는…,%CNAME(MASTER, TARGET)%의 일, 이렇게……」
		PRINTFORMDW %CALLNAME:TARGET%(은)는 당장 울기 시작할 것 같다…
		RETURN 1
	ENDIF

	;볼개그装備中
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;その他
	IF 0
	ELSEIF FAV_RATING
		PRINTFORMW 「화내요！？ 당연하겠지요」
	ELSEIF AFFECTION("익애", (FLAG:주인님)) && TALENT:TARGET:주인님
		PRINTFORMW 「부탁 이제 그만두어！ 그 이상, 말하지 마…」
	ELSE
		PRINTFORMW 「별로？ 분노는 하지 않지만. 그렇지만…」
	ENDIF
	RETURN 1
ENDIF

;REACT1_3
;「もう酷いことしなければ、もっと好きになれるかもしれない」 // REACT분류が적극적으로 한다で죠교대상호의が少ない場合
;========================================================== // 若しくはREACT분류が태연하게 한다
IF TFLAG:ACT파생 == 3
	;볼개그装備中
	IF TEQUIP:볼개그
		CALL PRINT_REACT_CATEGORY1_1_K39
		RETURN 1
	ENDIF

	;その他
	IF 0
	ELSEIF FAV_RATING
		PRINTFORMW 「 나, 그렇게 심한 금년이라고 왔는지 해들…」
	ELSEIF AFFECTION("익애", (FLAG:주인님)) && TALENT:TARGET:주인님
		PRINTFORML 「그렇게, 입니까…」
		PRINTFORMW 「 나, 조금은 기대해도 될까요」
	ELSE
		PRINTFORML 「그렇습니까…？」
		PRINTFORMW 「 그렇지만, 응. 곤란하군요…」
	ENDIF
	RETURN 1
ENDIF

;REACT1_4
;はっきり好きだと告げた // REACT분류が적극적으로 한다で、죠교대상호의가1000이상
;======================
IF TFLAG:ACT파생 == 4
	;初回
	IF FIRSTREACT(40)
		IF FAV_RATING
			PRINTFORML 「예, 알았어？」
			CALL FADE_TO_CLEARLINE(20, "「クスクス…」")
			CALL FADEIN(15, "「그렇지만…」")
			PRINTFORMW 「정말로 기쁘다……여기서 고마워요라고 말하는 것도 이상하지만…」
			PRINTFORMW 「고마워요, 이 일은 잊지 않아요. 쭉……」
