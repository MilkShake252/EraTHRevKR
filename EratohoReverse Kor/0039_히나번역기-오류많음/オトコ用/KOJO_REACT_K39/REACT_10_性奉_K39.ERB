FAV_RATING  = GET_KFLAG_K39("상사상애")
VOICEDDOT  '= PALAM:쾌Ｖ >= 8500 ? "゛" # "" 
AHEART     '= HTML_AH_K39()
EHEART     '= HTML_PS2_K39()

CALL QUAKE(4)
IF 0
;互いに정욕
ELSEIF IS_CONDITION(MASTER, "정욕") && IS_CONDITION(TARGET, "정욕")
	{
	CALL HTMLPRINTW(@"「…%RAND_SPLIT("읏/우/아/")%あ%AHEART% あ%VOICEDDOT%あぁっ%AHEART% " + 
;OriginalString : もう、%RAND_SPLIT("へんに/おかしく")%\@ FAV_RATING ? なっちゃ、う # な…る \@……」")
	@"이제(벌써),%RAND_SPLIT("이상하게/이상하고")%\@ FAV_RATING ? 낫챠, 우 # …루 \@……」")
	}
	CALL QUAKE(4)
	;ACT/REACTが항문섹스시킨다
	IF IS_NOWACTNAME("항문섹스시킨다")
		CALL HTMLPRINTW(@"「이, 이런 건은！ 나……엉덩이%RAND_SPLIT(", 뭔가로/그래서, 이렇게")%っ%EHEART%」")
	;その他
	ELSE
		{
		CALL HTMLPRINTW(@"「히앗%AHEART% 아앗%AHEART% " + 
;OriginalString : %RAND_SPLIT("あ/は")%あぁぅっ、あ%VOICEDDOT%ぁっ%AHEART% はぁっ…%EHEART%」")
		@"%RAND_SPLIT("아/하")%아아웃, 아%VOICEDDOT%앗%AHEART% 하앗…%EHEART%」")
		}
	ENDIF
;죠교대상が정욕
ELSEIF IS_CONDITION(MASTER, "정욕")
	{
	CALL HTMLPRINTW(@"「야,%RAND_SPLIT("격렬해져/안돼")%ッ！ 壊れるっ、壊れちゃうっ%AHEART% " + 
;OriginalString : あぅっ%AHEART% あ%VOICEDDOT%ぁっ%EHEART%」")
	@"아웃%AHEART% 아%VOICEDDOT%앗%EHEART%」")
	}
	CALL QUAKE(4)
	{
	CALL HTMLPRINTW(@"「앗%AHEART% 아%VOICEDDOT%앗%AHEART% 앗%AHEART% 이, 이것 안돼엣\@ 
	PALAM:쾌Ｖ >= 8500 ? ！！ # \@！」")
	}
	PRINTL
;죠교자が정욕
ELSEIF IS_CONDITION(TARGET, "정욕")
	{
	CALL HTMLPRINTW(@"「응 좀 더！ 좀 더 찔럿\@ 
	TENSION(TARGET, 8) == "高い" ? ！！！ # ！\@ あぅっ、あぁっ%EHEART%」")
	}
	CALl QUAKE(4)
	CALL HTMLPRINTW(@"「좀 더%AHEART% %RAND_SPLIT("질척질척/엄청")%に…%RAND_SPLIT("っ/ッ/")%！」")
ELSE
	;none
ENDIF
PRINTL
