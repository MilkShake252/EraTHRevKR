・TEQUIP:성교봉사중が立っている際にREACT분류が날뛴다だと拒絶扱いになる(即ち、성교봉사系ACT以外でも拒絶処理となる場合がある)
[SKIPEND]
@KOJO_MALE_REACT_K39_95 ;TFLAG:ACT파생(0=통상/1=연속/2=Ｖ성교봉사系から/3=Ａ성교系から/4=Ｖ성교系から/5=Ａ성교봉사系から)
#DIMS DYNAMIC AHEART
#DIMS DYNAMIC EHEART
#DIMS DYNAMIC MSTR
#DIMS DYNAMIC YOU
#DIMS DYNAMIC TGT
AHEART '= HTML_AH_K39()
EHEART '= HTML_PS2_K39()
MSTR   '= CNAME(MASTER, TARGET)
YOU    '= CALLNAME:MASTER
TGT    '= CALLNAME:TARGET

;조교중初回かつ受容である場合
;============================
;ONCEREACTではなくONCEを使用
;ONCEは번호を統一し、分類全体でターンにつき一度のみ表示する
IF IS_COMGRONAME("소극적으로 한다/태연하게 한다/적극적으로 한다/受け入れる/용서를 빌다") && ONCE(295)
	CALL TEXTDISPLAY_CATEGORY_10_ONCE_K39
	RETURN RESULT
ENDIF

;애태움開始
;==========
IF TCVAR:MASTER:가버려 && TCVAR:MASTER:애태움도 > 1 && IS_COMGRONAME("적극적으로 한다/태연하게 한다/受け入れる")
	CALL TEXTDISPLAY_CATEGORY_10_TEASE_K39
	RETURN RESULT
ENDIF

;쾌감을 참는다 // 55%
;===============
IF 구상記入チェック(55) && IS_NOWCOMNAME("쾌감을 참는다")
	CALL TEXTDISPLAY_CATEGORY_10_COM14_K39
	RETURN RESULT
ENDIF

;消極的に従った // 50%
;==============
IF 구상記入チェック(50) && IS_COMGRONAME("소극적으로 한다")
	CALL TEXTDISPLAY_CATEGORY_10_REACTCATEGORY_1_K39
	RETURN RESULT
ENDIF

;쾌감을 받아들인다 // 60%
;================
IF 구상記入チェック(60) && IS_NOWCOMNAME("쾌감을 받아들인다")
	CALL TEXTDISPLAY_CATEGORY_10_COM15_K39
	RETURN RESULT
ENDIF

;허리를 흔든다
;========
IF IS_NOWCOMNAME("허리를 흔든다")
	;죠교대상が가버려 // 애태움中は上層で処理しているのでこの箇所では考慮しない
	;------------------
	IF TCVAR:MASTER:가버려
		;애태움終了
		IF TCVAR:MASTER:애태움도 == 1
			;関数で汎用処理, COM20_1
			CALL PRINT_REACT_CATEGORY_10_COM20_1_K39
			PRINTFORMDL %RAND_SPLIT("자궁입까지 후벼파진다감각에/질내의 최안쪽까지 켜진다감각에")%,
		;애태움無し
		ELSE
			;汎用処理, COM20_2
			CALL PRINT_REACT_CATEGORY_10_COM20_2_K39
			{
			PRINTFORMDL %RAND_SPLIT(
;OriginalString : 子宮입に届くほどの深く激しい突きこみに/" + 
			"자궁입에 닿을 정도의 깊고 격렬한 공격에/" + 
;OriginalString : %YOU%の激しい허리기술に/" + 
			@"%YOU%의 격렬하다허리기술에/" + 
;OriginalString : 荒々しい허리기술で抽送を叩きこまれ")%、
			"몹시 거칠다허리기술그리고 삽입질을 주입해지고")%,
			}
		ENDIF
		PRINTFORMDW %TGT%하%PLEASURE_DESCRIPTION(TARGET)%허덕여 번민이라고 있다…
		RETURN 1
	ENDIF

	;互いに정욕
	;----------
	IF IS_CONDITION(MASTER, "정욕") && IS_CONDITION(TARGET, "정욕")
		CALL QUAKE(4)
		CALL HTMLPRINTW(@"「…읏아%AHEART% %RAND_SPLIT("아/응/후/")%あぁっ%AHEART% もう、おかしくな…る……」")
		CALL QUAKE(4)
		{
		CALL HTMLPRINTW(@"「히앗%AHEART% 아앗%AHEART% " + 
;OriginalString : %RAND_SPLIT("あ/は")%あぁぅっ、あぁっ%AHEART% はぁっ…%EHEART%」")
		@"%RAND_SPLIT("아/하")%아아웃, 아앗%AHEART% 하앗…%EHEART%」")
		}
		PRINTFORMDW 정욕에 고두 명은 서로 서로 미칠 듯하게 요구해 , 짐승과 같이 격렬하게 사귀고 있다…
		RETURN 1
	ENDIF

	;죠교대상が정욕
	;--------------
	IF IS_CONDITION(MASTER, "정욕")
		CALL QUAKE(4)
		CALL HTMLPRINTW(@"「겨, 격렬해져！ 망가진닷, 망가져 버린닷%AHEART% 아웃%AHEART% 아앗%EHEART%」")
		CALL QUAKE(4)
		CALL HTMLPRINTW(@"「앗%AHEART% 아앗%AHEART% 앗%AHEART% 이, 이것 안돼엣\@ PALAM:쾌Ｖ >= 8500 ? ！！ # \@！」")
		PRINTL
		PRINTFORMDL 정욕에 고%YOU%의 %RAND_SPLIT("격/황들")%강요하고 찔러 포함에,
		PRINTFORMDW %TGT%하%PLEASURE_DESCRIPTION(TARGET)%격렬하게 허덕이고 있다…
		RETURN 1
	ENDIF

	;죠교자が정욕
	;------------
	IF IS_CONDITION(TARGET, "정욕")
		CALL QUAKE(4)
		{
		CALL HTMLPRINTW(@"「응 좀 더！ 좀 더 찔럿\@ 
		TENSION(TARGET, 8) == "高い" ? ！！！ # ！\@ あぅっ、あぁっ%EHEART%」")
		}
		CALl QUAKE(4)
		CALL HTMLPRINTW(@"「좀 더%AHEART% %RAND_SPLIT("질척질척/엄청")%に…%RAND_SPLIT("っ/ッ/")%！」")
		PRINTFORMDL 정염에 몸을 태운다%TGT%하%YOU%(을)를 격렬하게 요구하면서,
		PRINTFORMDW 그 젖은 입술로부터 요염한 선율을 연주하고 있다…
		RETURN 1
	ENDIF

	;その他 // この箇所のみ確率で汎用処理に流す
	;------
	IF 구상記入チェック(65)
		CALL QUAKE(4)
		;상사상애
		IF GET_KFLAG_K39("상사상애")
			{
			CALL HTMLPRINTW(@"「읏%AHEART% 아%RAND_SPLIT("응/")%んっ%AHEART% " + 
;OriginalString : %RAND_SPLIT("あ/ふ/")%ぁっ%AHEART% 好きっ%AHEART% 大好きっ%EHEART%」")
			@"%RAND_SPLIT("아/후/")%앗%AHEART% 좋앗%AHEART% 너무 좋앗%EHEART%」")
			}
			CALL QUAKE(4)
			PRINTFORMW 「응, 키스 해…모두 잊게 해……」
			CALL CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K39(, , "키스", 1)
		;익애
		ELSEIF AFFECTION("익애")
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("아/응/후/아아/응아/후아")%ぁっ%AHEART% " + 
;OriginalString : %RAND_SPLIT("は/あ/")%%RAND_SPLIT("う/うぅ/ぅ")%っ%AHEART% " + 
			@"%RAND_SPLIT("하/아/")%%RAND_SPLIT("우/우우/우")%읏%AHEART% " + 
;OriginalString : あっ%AHEART% あっ%AHEART% あぁっ…%EHEART%」")
			@"앗%AHEART% 앗%AHEART% 아앗…%EHEART%」")
			}
			CALL QUAKE(4)
			PRINTFORMW 「부탁해요, 키스 해……%KOJO_RAND_K39(("키스"), 1)%」
		;その他
		ELSE
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("응/")%んっ%AHEART% " + 
;OriginalString : あ%RAND_SPLIT("ァッ/ぁっ/ぁッ")%%AHEART% 凄く、深くて…」")
			@"아%RAND_SPLIT("/앗/앗")%%AHEART% 굉장히, 깊어서…」")
			}
			CALL QUAKE(4)
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("응아/아아/후아/응/아/후/")%ぁっ%AHEART% " + 
;OriginalString : はうぅっ！ あっ%AHEART% あっ、あぁっ…%EHEART%」")
			@"하우웃！ 앗%AHEART% 아, 아앗…%EHEART%」")
			}
		ENDIF
		RETURN 1
	ENDIF
ENDIF

