DRAWLINE

IF RESULT == 00
	CALL FILTER_TALK
ELSEIF RESULT == 10
	CALL FILTER_TOUCH
ELSEIF RESULT == 20
	CALL FILTER_TOYS
ELSEIF RESULT == 30
	CALL FILTER_RAPE
ELSEIF RESULT == 40
	CALL FILTER_DARKHISTORY
ELSEIF RESULT == 50
	CALL FILTER_FOOT_BATHS
ELSEIF RESULT == 60
	CALL FILTER_VIOLENCE
ELSEIF RESULT == 70
	CALL FILTER_LUNATIC
ELSEIF RESULT == 80
	PRINTW SHOP 화면으로 돌아갑니다.
	CALL SHOP("SHOW_SHOP")
ELSEIF RESULT == 90
	CALL FILTER_REST_AND_RAPE
ELSEIF RESULT == 999
	CALL SHOP("SHOW_SHOP")
ENDIF

