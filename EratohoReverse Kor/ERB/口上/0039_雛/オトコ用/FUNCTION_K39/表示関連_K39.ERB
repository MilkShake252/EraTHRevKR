;함수명:COLOR_K39
;개  요점:@RGBCOLOR에 대신해, 캐릭터 라인에 의한 색지정을 실시한다
;인  수:ARGS… 지정하는 색의 이름, 또는 해당 문자색의 캐릭터명
;반환값:RGBCOLOR 지정의 값
;비  고:식 안의 함수
;사용예:CALL PRINTCOLORW(COLOR2_K39("white"), " 『이 캐릭터 라인은 흰색으로 표시되고 개행 기다려 한다』")
;-------------------------------------------------
@COLOR_K39(ARGS)
#FUNCTION
SELECTCASE ARGS
	;HEART용. 조교자의만족도에 응해 색을 5% 잘게 썰기로변화시킨다
	CASE "핑크", "pink"
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				RETURNF RGBCOLOR(255, 179, 222)
			CASE "일반"
				RETURNF RGBCOLOR(255, 191, 235)
			CASE "낮다"
				RETURNF RGBCOLOR(255, 204, 247)
		ENDSELECT
	CASE "히나", "HINA", "red", "세키"
		RETURNF RGBCOLOR(181, 33, 59)
	CASE "흰색", "white"
		RETURNF RGBCOLOR(192, 192, 192)
	CASE "흑", "black"
		RETURNF RGBCOLOR(0, 0, 0)
	CASE "유카리", "YUKARI"
		RETURNF RGBCOLOR(204, 153, 255)
	CASE "앨리스", "ALICE"
		RETURNF RGBCOLOR(54, 166, 255)
	CASE "사쿠야", "SAKUYA"
		RETURNF RGBCOLOR(172, 175, 210)
	CASE "샹하이", "test"
		RETURNF RGBCOLOR(150,200,150)
	CASEELSE
		CALLF PRINT_ERRORMESSAGE("@COLOR_K39에 무효인 정의(%ARGS%)가 도 사라질 수 있는 했다")
		RETURNF -1
ENDSELECT

