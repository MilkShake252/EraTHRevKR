;SETFONT_K39
;폰트를나눔명조, 폰트 스타일을 굵은 글씨, 이탤릭으로 한다
;현상, 용도가 한정되기 때문에 1대의 비행기노우에 사무치는 일에. 확장했다구 되면 생각한다
;-------------------------------------------------
@SETFONT_K39
SIF CHKFONT("나눔명조")
	SETFONT "나눔명조"
FONTSTYLE 3

;-------------------------------------------------
;CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K39
;인  수:ARG:0 =시색, 생략 하면히나의 문자색
;인  수:ARG:1 =종색, 생략 하면 흑
;인  수:ARG:2 = 1으로 TARGET, 2로 MASTER의상태변화를 참조한다
;인  수:ARG:3 = -2이외라면 FADE가 OFF인 경우에 RESETCOLOR로 PRINT를 출력했다 후, 구상주의 문자색에 되돌린다
;인  수:ARGS  = @KOJO_RAND_K39의 행위의 분류를 지정한다. 캐릭터 라인 인수
;
;	FADEOUT로서의 사용이 주로 된다고 생각되므로 pre-set는 FADEOUT용. FADEIN라고 해도 아무튼 사용할 수 있다.
;	이런 응범용화해도 아무도 사용하지 않을 것 같다(용도도 없는 것 같다)이니까, 다른 일은 상정하지 않는다. 자신의 사용승손우선.
;	원래@KOJO_RAND_KX의 존재가 전제이고w
;-------------------------------------------------
@CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K39(ARG:0 = 0xA8142F, ARG:1 = 0x000000, ARGS, ARG:2 = 1, ARG:3 = -2)
IF ANIME_CONFIG("FADE")
	CALL FADE(20, ARG:0, ARG:1, @"　　　　　%KOJO_RAND_K39((ARGS), ARG:2)%")
ELSE
	SIF ARG:3 != -2
		RESETCOLOR
;OriginalString : \@ ARG:3 == -2 ? 「 # （ \@%KOJO_RAND_K39((ARGS), ARG:2)%\@ ARG:3 == -2 ? 」 # ） \@
	PRINTFORMW \@ ARG:3 == -2 ? 「 # （ \@%KOJO_RAND_K39((ARGS), ARG:2)%\@ ARG:3 == -2 ? 」 # ） \@
	SIF ARG:3 != -2
		CALL KOJO_COLOR(TARGET)
ENDIF
CALL FADE_CLINE(1)


;히나님은 텐션 높을 때는 일인칭이"히나"가 된다든가 말하는 나설정 함수
;==============================================================
@TNAME_K39(ARG:0 = -2) ;//인수를 생략 하면 TARGET
#FUNCTIONS
#LOCALSSIZE 1
VARSET LOCALS
ARG:0 = ARG:0 != -2 ? ARG:0 # TARGET
LOCALS:0 '= IS_CONDITION(ARG:0, "정욕") || TENSION(ARG:0, 8) == "높다" ?"히나" # "나" 
RETURNF LOCALS:0


;PRINT_SYMBOL의 HTMLFONT판
;========================
@HTML_PS_K39(ARG:0 = -2) ;//인수를 생략 하면 TARGET
#FUNCTIONS
#LOCALSSIZE 1
VARSET LOCALS
ARG:0 = ARG:0 != -2 ? ARG:0 # TARGET
;//기본적으로 행말에서의 사용을 상정

[SKIPSTART]
호의, 호감도로 파생한다
하트계의 출력은 2/3으로 HEARTB, 1/3그리고 HEART를 출력한다
음표계의 출력은 2/3으로 8분 음표, 1/3그리고 련 8분 음표를 출력한다
[SKIPEND]
IF GET_KFLAG_K39("상사상애", ARG:0)
	;BASE:만족로 파생한다
	SELECTCASE TENSION(ARG:0, 8)
		;BASE:만족가 50%이상
		CASE "높다"
			;1/4로 하트 2개를 출력
			IF PERCENT(25)
				LOCALS:0 '= RAND:3 ? HTML_HEARTB(, COLOR2_K39("PINK"), 2) # HTML_HEART(, COLOR2_K39("PINK"), 2)
			;그 외라면 하트 1개
			ELSE
				LOCALS:0 '= RAND:3 ? HTML_HEARTB(, COLOR2_K39("PINK")) # HTML_HEART(, COLOR2_K39("PINK"))
			ENDIF
		;BASE:만족가 25%이상
		CASE "일반"
			LOCALS:0 '= RAND:3 ? HTML_EIGHTHNOTE() # HTML_W8NOTE()
		;BASE:만족가 25% 미만
		CASE "낮다"
			;아무것도 출력하지 않는다
			LOCALS:0 '= ""
	ENDSELECT
ELSEIF AFFECTION("익애", ARG:0)
	SELECTCASE TENSION(ARG:0, 8)
		CASE "높다"
			;1/4로 하트계를 출력
			IF PERCENT(25)
				LOCALS:0 '= RAND:3 ? HTML_HEARTB(, COLOR2_K39("PINK")) # HTML_HEART(, COLOR2_K39("PINK"))
			;그 외라면 음표계를 출력
			ELSE
				LOCALS:0 '= RAND:3 ? HTML_EIGHTHNOTE() # HTML_W8NOTE()
			ENDIF
		CASE "일반"
				LOCALS:0 '= RAND:3 ? HTML_EIGHTHNOTE() # HTML_W8NOTE()
		CASE "낮다"
			LOCALS:0 '= ""
	ENDSELECT
ELSE
	SELECTCASE TENSION(ARG:0, 8)
		CASE "높다"
			LOCALS:0 '= HTML_EIGHTHNOTE()
		CASEELSE
			LOCALS:0 '= ""
	ENDSELECT
ENDIF
RETURNF LOCALS:0


