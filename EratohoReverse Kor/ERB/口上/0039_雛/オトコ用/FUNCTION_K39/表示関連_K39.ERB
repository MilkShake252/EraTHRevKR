;AUTOHEARTCB의 HTMLFONT판
;=======================
@HTML_AH_K39(ARG:0 = -2) ;//인수를 생략 하면 TARGET
#FUNCTIONS
VARSET LOCALS
ARG:0 = ARG:0 != -2 ? ARG:0 # TARGET
;//기본적으로 행중에서의 사용을 상정

;호의, 호감도로 파생한다
;하트계의 출력은 2/3으로 HEARTB, 1/3그리고 HEART를 출력한다
IF GET_KFLAG_K39("상사상애", ARG:0)
	;2/3로 하트를 출력
	IF RAND:3
		;BASE:만족가 50%이상이라면 하트 2개를 출력
		IF TENSION(ARG:0, 8) == "높다"
			LOCALS:0 '= RAND:3 ? HTML_HEARTB(, COLOR2_K39("PINK"), 2) # HTML_HEART(, COLOR2_K39("PINK"), 2)
		;그 외라면 하트 1개
		ELSE
			LOCALS:0 '= RAND:3 ? HTML_HEARTB(, COLOR2_K39("PINK")) # HTML_HEART(, COLOR2_K39("PINK"))
		ENDIF
	;1/3로 exclamation
	ELSE
		LOCALS:0 '= "!"
	ENDIF
ELSEIF AFFECTION("익애", ARG:0) || AFFECTION("조교완료", ARG:0)
	;BASE:만족가 50%이상시 1/3의 확률로 하트를 출력
	IF TENSION(ARG:0, 8) == "높다" && !RAND:3
		LOCALS:0 '= RAND:3 ? HTML_HEARTB(, COLOR2_K39("PINK")) # HTML_HEART(, COLOR2_K39("PINK"))
	;그렇지 않으면 exclamation
	ELSE
		LOCALS:0 '= "!"
	ENDIF
ELSE
	LOCALS:0 '= "!"
ENDIF
RETURNF LOCALS:0


