;-------------------------------------------------
;KFLAG를 정의. 미정도리의 것이 도당했다등 에러 빠짐
;-------------------------------------------------
@GET_KFLAG_K39(ARGS, ARG = -2)
#FUNCTION
ARG = ARG != -2 ? ARG # TARGET
SELECTCASE ARGS
	CASE "상사상애"
		RETURNF KFLAG:ARG:100 == 1
	CASE "ED경과"
		RETURNF KFLAG:ARG:150 == 1
	CASE "성봉판정"
		RETURNF KFLAG:ARG:200 == 1
	CASEELSE
		CALLF PRINT_ERRORMESSAGE(@"@GET_KFLAG_K39에 무효인 정의가 도 사라질 수 있는 했다")
		RETURNF -1
ENDSELECT
