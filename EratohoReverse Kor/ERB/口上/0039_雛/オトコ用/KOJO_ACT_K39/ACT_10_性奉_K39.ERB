			CASE 2
				CALL HTMLPRINTW(@"「저기, 와……%LOCALS:0%최후는 역시, 나의 안에서 받아 들여 주고 싶고……」")
			CASE 3
				CALL HTMLPRINTW(@"「좀 더참다 해요%LOCALS:0%어차피라면, 나의 안에서 실시했으면 좋고…」")
		ENDSELECT
	ELSE
		SELECTCASE AUTORAND(3, 197)
