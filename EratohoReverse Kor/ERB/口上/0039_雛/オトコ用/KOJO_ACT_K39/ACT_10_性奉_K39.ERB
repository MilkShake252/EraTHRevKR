	;==========
	IF !TCVAR:MASTER:애태움도
		;조교자가가버려
		;================
		IF PALAM:쾌Ｖ > 8500
			CALL QUAKE(4)
			{
			CALL HTMLPRINTW(
			@"「후앗, 아앗%HTML_AH_K39()%  아, 앗" + 
