;────────────────────────────────────
;EV21, 대만족구상
;────────────────────────────────────
;·조교자의대만족보너스를취득했다경우에 표시
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_21(ARG)
;-------------------------------------------------
#LOCALSIZE 1
VARSET LOCAL
LOCAL:0 = GET_KFLAG_K39("상사상애")

;━━━━━━━━━━━━━━━━━━━━━━━━
;히나님대만족
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100)
	;=================================================
	;상냥하게 응답한다
	;=================================================
	IF IS_NOWCOMNAME("상냥하게 응답한다")
;OriginalString : 「\@ LOCAL:0 ? うふっ、何かご褒美を考えなくちゃね # 愛想がいいのが一番ですね \@
		PRINTFORM 「\@ LOCAL:0 ? 우훗, 뭔가 포상을 생각하지 않으면 말야 # 애교가 있는 것이 제일이군요 \@
		CALL PRINT_SYMBOL_K39
;OriginalString : 히나は慈愛に満ちた表情で%CALLNAME:MASTER%を見つめている…
		PRINTFORMDW 병아리는 자애로 가득 찬 표정으로 %CALLNAME:MASTER%(을)를 응시하고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;쾌감을 받아들인다
	;=================================================
	IF IS_NOWCOMNAME("쾌감을 받아들인다")
;OriginalString : 「\@ LOCAL:0 ? うふっ、可愛い # 솔직な子は好きですよ \@
		PRINTFORM 「\@ LOCAL:0 ? 우훗, 사랑스럽다 # 솔직아이는 좋아해요 \@
		CALL PRINT_SYMBOL_K39
;OriginalString : 히나は微笑みながら%CALLNAME:MASTER%を見つめている…
		PRINTFORMDW 병아리는 미소지으면서%CALLNAME:MASTER%(을)를 응시하고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;비명지른다
	;=================================================
	IF IS_NOWCOMNAME("비명지른다")
;OriginalString : 「\@ LOCAL:0 ? ねぇ、もっと。もっと聞かせて # そんな甘い声で鳴かれると私…、ちょっと興奮してしまいます \@……」
		PRINTFORMW 「\@ LOCAL:0 ? 저기, 좀 더. 좀 더 들려줘 # 그런 달콤한 소리로 울어지면 나…, 조금 흥분해 버립니다 \@……」
		RETURN 1
	ENDIF
	
	;=================================================
	;애정을 담아 한다
	;=================================================
	IF IS_NOWCOMNAME("애정을 담아 한다")
;OriginalString : 「\@ LOCAL:0 ? そんなに優しくされると蕩けちゃう # そんなに優しく당했다ら、蕩けてしまいます \@…」
		PRINTFORMW 「\@ LOCAL:0 ? 그렇게 상냥하게 여겨지면 녹아 버린다 # 그렇게 상냥하고당했다노, 녹아 버립니다 \@…」
;OriginalString : 히나は潤んだ瞳で%CALLNAME:MASTER%を見つめている…
		PRINTFORMDW 병아리는 물기를 띤 눈동자로%CALLNAME:MASTER%(을)를 응시하고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;적극적으로 한다(적극적으로 한다, 이루는대로등)
	;=================================================
	IF 구상기입체크(100) && IS_COMGRONAME("적극적으로 한다")
		;-------------------------------------------------
		;ED경과
		;-------------------------------------------------
		IF GET_KFLAG_K39("ED경과", TARGET)
;OriginalString : 「好き、大好き…」
			PRINTFORMW 「좋아해, 너무 좋아…」
;OriginalString : 히나は潤んだ瞳で%CALLNAME:MASTER%を見つめている…
			PRINTFORMDW 병아리는 물기를 띤 눈동자로%CALLNAME:MASTER%(을)를 응시하고 있다…
			RETURN 1
		ENDIF
		
		;---------------------
		;그 외
		;---------------------
		;상사상애
		IF GET_KFLAG_K39("상사상애")
;OriginalString : 「こんな事、駄目なのは分かってる。でも…」
			PRINTFORMW 「이런 일, 안된 것은 알고 있다. 그렇지만…」
;OriginalString : 「\@ RAND:2 ? もう、 # %CNAME(MASTER, TARGET)%と \@離れたくない……」
			PRINTFORMW 「\@ RAND:2 ? 이제(벌써), # %CNAME(MASTER, TARGET)%(와)과 \@떨어지고 싶지 않다……」
;OriginalString : 히나は潤んだ瞳で%CALLNAME:MASTER%を見つめている…
			PRINTFORMDW 병아리는 물기를 띤 눈동자로%CALLNAME:MASTER%(을)를 응시하고 있다…
		;그 외
		ELSE
;OriginalString : 「적극적으로求めてくれると、私も嬉しいです…」
			PRINTFORMW 「적극적으로 요구해 주면, 나도 기쁩니다…」
;OriginalString : 히나は微笑みながら%CALLNAME:MASTER%を見つめている…
			PRINTFORMDW 병아리는 미소지으면서%CALLNAME:MASTER%(을)를 응시하고 있다…
		ENDIF 
		RETURN 2
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	CALL HTMLPRINTW(@"「\@ LOCAL:0 ? 킥킥, 좋은 느낌이군요 # 후후, 나쁘지 않네요 \@…%HTML_HEARTB(, COLOR2_K39("PINK"))%」")
	RETURN 1
ENDIF

;────────────────────────────────────
;EV22, 상태변화구상
;────────────────────────────────────
;·상태이상가 붙었을 때에 표시
;·TCVAR:상태변화가조교자, TCVAR:MASTER:상태변화가조교대상
;·상태변화가 일어나면 3 턴지속 한다
;-------------------------------------------------
@KOJO_MALE_EVENT_K39_22(ARG)
;-------------------------------------------------
;·가까운 장래, 조교자와조교대상로상태변화구상호출을 분리하고 싶은 의향
;·그리고상태변화 이외의발기묘사, 윤활묘사, 소변묘사 근처에도구상호출의 테두리를 추가했다 있고 생각하고(2015/01/09)
;-------------------------------------------------
;*************************************************
;EV22_0, 조교자상태변화
;*************************************************
IF 구상기입체크(100) ;&& ARG == 0
	;조교자의상태변화로 파생
	SELECTCASE GET_CONDITION(TARGET)
		CASE "피폐"
			;상태변화지속턴을 2에
			TCVAR:상태변화지속 = 2
			;2/3의확률로표시없음 RESULT에 0을반환
			IF RAND:3
				RETURN 0
			;1/3의확률로표시
			ELSE
;OriginalString : 「\@ GET_KFLAG_K39("상사상애") ? ちょっと%RAND_SPLIT("やりすぎかしら/疲れたわね")% # ちょっと疲れま했다ね \@…」
				PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 조금%RAND_SPLIT("너무 할까/지쳤군요")% # 조금 지쳤어요 \@…」
;OriginalString : 히나は肩で大きく息をしている…
				PRINTFORMDW 병아리는 어깨로 크게 숨을 쉬고 있다…
				RETURN 1
			ENDIF
		CASE "쇠약"
			RETURN 0
		CASE "무기력"
			RETURN 0
		CASE "몽롱"
			RETURN 0
		CASE "정욕"
			IF RAND:3
				RETURN 0
			ELSE
;OriginalString : 「\@ GET_KFLAG_K39("상사상애") ? も、もう駄目……。참다、できないわ…… # ご免なさい、私、もう참다できませんっ！ \@」
				PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 이, 이제 안돼……참다, 할 수 없어요…… # 미안해요, 나, 이제(벌써)참다할 수 없습니다!  \@」
;OriginalString : 히나は정욕に濁った目で%CALLNAME:MASTER%を見据えながら近寄ってきた…
				PRINTFORMDW 병아리는 정욕에 탁해진 눈으로 %CALLNAME:MASTER%(을)를 응시하면서 접근해 왔다…
				RETURN 1
			ENDIF
		CASE "분노"
			IF AFFECTION("상사상애")
				TCVAR:상태변화지속 = 1
			ELSEIF AFFECTION("익애")
				IF RAND:3
					TCVAR:상태변화지속 = 2
				ELSE
					TCVAR:상태변화지속 = 1
				ENDIF
			ELSEIF AFFECTION("권애")
				IF RAND:3
					TCVAR:상태변화지속 = 3
				ELSE
					TCVAR:상태변화지속 = 2
				ENDIF
			ELSE
				TCVAR:상태변화지속 = 3
			ENDIF
			IF RAND:3
				RETURN 0
			ELSE
;OriginalString : 「\@ GET_KFLAG_K39("상사상애") ? ねぇ、本気で怒ってもいいの # 私を怒らせて楽しいですか \@？」
				PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 저기, 정말로 화내도 괜찮은거야 # 나를 화나게 해 즐겁습니까 \@?」
				RETURN 1
			ENDIF
		CASE "싫증"
			;조교의경과시간
			SELECTCASE TFLAG:경과시간
				;길다
				CASE IS >= 60
;OriginalString : 「\@ GET_KFLAG_K39("상사상애") ? ねぇ、もう終わりにする # そろそろ止めましょうか \@…？」
					PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 저기, 벌써 끝낸다 # 슬슬 멈출까요 \@…?」
					TCVAR:상태변화지속 = 3
				;보통
				CASE IS >= 30
;OriginalString : 「\@ GET_KFLAG_K39("상사상애") ? なんかいまひとつな感じ # どうもいまひとつですね \@…」
					PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 뭔가 하나 더느낌 # 아무래도 지금 하나군요 \@…」
					TCVAR:상태변화지속 = 2
				;짧다
				CASEELSE
;OriginalString : 「\@ GET_KFLAG_K39("상사상애") ? なんか気分が乗らない感じ # どうも気分が乗りませんね \@…」
					PRINTFORMW 「\@ GET_KFLAG_K39("상사상애") ? 뭔가 기분이 타지 않는 느낌 # 아무래도 기분이 타지 않네요 \@…」
					TCVAR:상태변화지속 = 1
			ENDSELECT
			RETURN 1
		CASE "울상"
			;//TARGET의 경우, TALENT:광기가 있는 경우에게만 1/5의 확률로울상화
			;//광기소유가 아닌 경우는 죽어구상가 된다
			RETURN 0
	ENDSELECT
ENDIF

