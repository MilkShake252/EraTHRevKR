;조교자Ｖ감각파생
SELECTCASE ABL:TARGET:Ｖ감각
	;Ｖ감각 8 이상
	CASE IS >= 8
		SELECTCASE AUTORAND(2, 301)
			CASE 0
				{
				CALL HTMLPRINTW(@"「하\@ PARAM >= 8500 ? # 
				\@응%AHEART% %RAND_SPLIT("응/")%! 아%VOICEDDOT%\@ 
				PARAM >= 8500 ? # 
				\@! 앗%AHEART% 아%EHEART%」")
				}
				{
				CALL HTMLPRINTW(@"「야\@ PARAM >= 8500 ? # 
