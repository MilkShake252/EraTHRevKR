		IF TCVAR:MASTER:애태움도 == 1
			;함수로 범용 처리, COM20_1
			CALL PRINT_REACT_CATEGORY_10_COM20_1_K39
;OriginalString : 背後から%RAND_SPLIT("の激しい突きこみに/激しく突き上げられ")%、
			PRINTFORMDL 배후로부터%RAND_SPLIT("의 격렬한 공격에/격렬하게 밀어올릴 수 있다")%,
;OriginalString : %TGT%は%PLEASURE_DESCRIPTION(TARGET)%喘ぎ悶えている…
			PRINTFORMDW %TGT%은(는) %PLEASURE_DESCRIPTION(TARGET)%허덕여 괴로워하고 있다…
			RETURN 1
		;애태움없음
		ELSE
			;함수로 범용 처리, COM20_2
			CALL PRINT_REACT_CATEGORY_10_COM20_2_K39
;OriginalString : 背後からの責めに被虐心を刺激され、%TGT%は%PLEASURE_DESCRIPTION(TARGET)%喘ぎ悶えている…
			PRINTFORMDW 배후로부터의 고문에 피학심이 자극되어%TGT%은(는) %PLEASURE_DESCRIPTION(TARGET)%허덕여 괴로워하고 있다…
		ENDIF
		RETURN 1
	ENDIF

	;서로정욕
	;----------
	IF IS_CONDITION(MASTER, "정욕") && IS_CONDITION(TARGET, "정욕")
		CALL KOJO_REACT_LUST_K39
;OriginalString : 정욕に滾る二人は互いに狂おしく求め合い、獣のように激しく交わっている…
		PRINTFORMDW 정욕에 고두 명은 서로 서로 미칠 듯하게 요구해 , 짐승과 같이 격렬하게 사귀고 있다…
		RETURN 1
	ENDIF

	;조교대상가정욕
	;--------------
	IF IS_CONDITION(MASTER, "정욕")
		CALL KOJO_REACT_LUST_K39
;OriginalString : 정욕に滾る%CALLNAME:MASTER%의 배후からの激しい突きこみに、
		PRINTFORMDL 정욕에 고%CALLNAME:MASTER%의 배후로부터의 격렬한 찌르기 포함에,
;OriginalString : %TGT%は%PLEASURE_DESCRIPTION(TARGET)%激しく喘いでいる…
		PRINTFORMDW %TGT%은(는) %PLEASURE_DESCRIPTION(TARGET)%격렬하게 허덕이고 있다…
		RETURN 1
	ENDIF

	;조교자가정욕
	;------------
	IF IS_CONDITION(TARGET, "정욕")
		CALL KOJO_REACT_LUST_K39
;OriginalString : 정욕に滾る%TGT%は上体を仰け反らせながら激しく腰を振り、쾌락を貪っている…
		PRINTFORMDW 정욕에 고%TGT%(은)는 상체를 위로 돌리고 뒤로 젖힐 수 있는이면서 격렬하게 허리를 거절해, 쾌락을 탐내고 있다…
		RETURN 1
	ENDIF

	;그 외//확률로 범용 리액션에
	;------
	IF 구상기입체크(65) ;65%
		CALL QUAKE(4)
		;호의호감도로 파생
		IF GET_KFLAG_K39("상사상애")
			SELECTCASE RANDREACT(2, 200)
				CASE 0
					CALL HTMLPRINTW(@"「하웃! 앗%AHEART% 아%AHEART% 개, 이 모습, 굉장히 흥분해 버린다앗!」")
					CALL QUAKE(4)
					{
					CALL HTMLPRINTW(@"「얏, 앗%AHEART% 아%AHEART% " + 
					@"%RAND_SPLIT("응/하//아")%%AHEART% 아%RAND_SPLIT("응/")%%EHEART%」")
					}
				CASE 1
					CALL HTMLPRINTW(@"「아앗!? 야앗%AHEART% 그런, 난폭하게!」")
					CALL QUAKE(4)
					{
					CALL HTMLPRINTW(@"「그, 그렇지만! 굉장히, 흥분해 버렷!  " + 
					@"응응%AHEART% 는%AHEART% 아%EHEART%」")
					}
			ENDSELECT
		ELSE
			SELECTCASE RANDREACT(2, 201)
				CASE 0
					CALL HTMLPRINTW(@"「읏, 아앗%AHEART% 뒤로부터 찔리는 것은, 흥분해 버립니다…」")
					CALL QUAKE(4)
					{
					CALL HTMLPRINTW(@"「…… 좋아요. 좀 더, 격렬하게 찔러도……%RAND_SPLIT("응/아")%," + 
					@"%RAND_SPLIT("/만난다/아")%%EHEART%」")
					}
				CASE 1
					CALL HTMLPRINTW(@"「%RAND_SPLIT("응아/응/아/응/")%%AHEART% , 거기! 만지작거려지면…!」")
					CALL QUAKE(4)
					CALL HTMLPRINTW(@"「아읏%AHEART% 후~%AHEART% 응응%AHEART% 아…%EHEART%」")
			ENDSELECT
		ENDIF
;OriginalString : 背後から%YOU%に突かれ、%TGT%は%PLEASURE_DESCRIPTION(TARGET)%喘いでいる…
		PRINTFORMDW 배후로부터%YOU%에 찔려%TGT%은(는) %PLEASURE_DESCRIPTION(TARGET)%허덕이고 있다…
		RETURN 1
	ENDIF
ENDIF
