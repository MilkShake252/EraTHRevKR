			}
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("/아")%%RAND_SPLIT("/")%%AHEART% " + 
			@"%RAND_SPLIT("는/아/")%%RAND_SPLIT("/")%%AHEART% " + 
			@"%RAND_SPLIT("아/응/하")%응%AHEART% 아%AHEART% 아%EHEART%」")
			}
