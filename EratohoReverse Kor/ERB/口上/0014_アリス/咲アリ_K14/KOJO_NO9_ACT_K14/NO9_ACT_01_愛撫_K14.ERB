;OriginalString : c이이하로수정 PRINTCOLOR(COLOR_K14("앨리스"), "「슬슬 결정타를 찔러 주어요…")
					CALL PRINTCOLOR(COLOR_K14("앨리스"), "「슬슬 결정타를 찔러줄게…」")
;OriginalString : c이이하로수정 PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
				CASEELSE ;애태움없음
;OriginalString : 「이제 때일까?」
					PRINTFORMW 「슬슬 때가 됬으려나?」
;OriginalString : c이이하로수정 PRINTCOLOR(COLOR_K14("앨리스"), "「그러면, 조금격렬하게 주어요…")
					CALL PRINTCOLOR(COLOR_K14("앨리스"), "「그러면, 조금 격렬하게 해 줄게…")
;OriginalString : c이이하로수정 PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			ENDSELECT
		;뒤로부터
		ELSE
			SELECTCASE TCVAR:MASTER:애태움도 ;애태움도파생 처리
				CASE IS > 1
