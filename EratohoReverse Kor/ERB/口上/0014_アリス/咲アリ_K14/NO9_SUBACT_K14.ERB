;========================================================================
;조교액션·커맨드구상
;========================================================================

;────────────────────────────────────
;탈의액션구상
;────────────────────────────────────
;·조교자액션의 앞에강제탈의나자주탈의가 발생했다때에 표시
;·이구상의 시점에서는 아직 TEQUIP는 갱신되어 있지 않다
;·첫회 분기는 「처음 보았다/보였다」란 할 수 없다.
;  숍 메뉴로 벗게 했던 적이 있을지도 모르고, 조교의 흐름으로 완전하게 벗게 하지 않고서노출만 하고 있는 경우도 있다.
;  어쩌면조수였던 때에 본 적이 있을지도 모른다. 그러한 의미에서는 간이적인 것이다.
;  엄밀한 의미에서의 「초견」플래그를 준비할지 어떨지는 요망하는 대로라고 하는 일로.
;-------------------------------------------------
@KOJO_NO9_MS_DATUI_K14
;-------------------------------------------------
;관련 변수…
;강제탈의:TFLAG:102  자주탈의:TFLAG:103
;=(-1, 강제탈의에 저항, 1, 겉옷, 2, 하반신겉옷, 3, 웃도리, 4, 상반신전부, 5, 하반신전부,
;  6, 악세서리─, 7, 악세서리─이외 모두, 8, 전라까지, 9, 속옷만)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의에 저항당했다 FIRSTDATUI(0)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:강제탈의 < 0
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(100) && FIRSTDATUI(0)
		PRINTFORMW 「뭐야, 거부할 생각이야? 어떻든 간에…」
		PRINTFORMW 「저항한다면 억지로 벗길 수 밖에」
		RETURN 1
	ENDIF
	
	;=================================================
	;스스로 벗음
	;=================================================
	IF 구상기입체크(100) && TFLAG:자주탈의
		PRINTFORMW 「할 수 없네…」
		PRINTFORMW 「뭐, 내가 벗겨 줄 수 있는데 말이지」
		RETURN 1
	ENDIF
	
	;-------------------------
	;이외
	;-------------------------
	;조교자BASE:초조함파생
	SELECTCASE TENSION(TARGET, 7)
		CASE "높다"
			PRINTFORMW 「또 그렇게 거부하는군요」
			PRINTFORMW 「사쿠야, 당신……, 나를 초조하게 만들지 말아요」
		CASE "일반"
			PRINTFORMW 「……귀찮은 사람이네」
			PRINTFORMW 「내게 억지로 벗겨지는게 취향인거야?」
		CASE "낮다"
			PRINTFORMW 「%CNAME(MASTER, TARGET)%, 어쩌면…」
			PRINTFORMW 「입은 채로 하는 것을 좋아하는걸까…?」
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;두 사람 모두 뭔가 벗은 FIRSTDATUI(10)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·주로섹스계행동의 예비 동작으로 발생한다
;------------------------------------------------
IF 구상기입체크(100) && TFLAG:강제탈의 > 0 && TFLAG:자주탈의 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;조교자가[정욕]또한전라되었다
	;=================================================
	IF 구상기입체크(100) && T_COND("정욕") && ZENRACHECK(0)
		PRINTFORMW 「이런 것, 이제 필요없어!」
		PRINTFORM 「후후후, 태어난 그대로의 모습으로 즐겨보자구요…
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;=================================================
	;조교자가조교대상를익애
	;=================================================
	IF 구상기입 체크(0) && AFFECTION("익애")
		PRINTFORMW 「자, 이것도 벗고, 이것도…」
		PRINTFORM 「후후…, 마음껏 즐겨보자…
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;-------------------------
	;그 외
	;-------------------------
	;조교자BASE:만족파생
	SELECTCASE TENSION(TARGET, 8)
		CASE "높다"
			PRINTFORMW 「역시 이렇게밖에 안되는 걸까.」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%의 체온을 느끼고 싶은데. 하긴 뭐, 내 몸은…」
			PRINTFORM 「조금 쌀쌀할지도…
			CALL PRINT_SYMBOL_K14
		CASE "일반"
			PRINTFORMW 「입은 채로 하는 것도 나쁘지 않은데…」
			PRINTFORMW 「역시 방해되는걸까? 후후…」
		CASE "낮다"
			PRINTFORMW 「준비는 됬을까…?」
			PRINTFORM 「그럼, 빠르게 시작\@ AFFECTION("익애") ? 하자 # 해볼까? \@
			IF AFFECTION("익애")
				CALL PRINT_SYMBOL_K14
			ELSE
				PRINTW 」
			ENDIF
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·악세서리─마셔 FIRSTDATUI(20)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·주로양말. 그 밖에장갑나소매 등
;------------------------------------------------
IF 구상기입체크(100) && TFLAG:자주탈의 == 6 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;전라되었다（벗기 전은양말 이외 알몸이라든지 그러한 느낌）
	;=================================================
	IF 구상기입체크(100) && TFLAG:자주탈의 && ZENRACHECK(0) && !TEQUIP:눈가리개
		SELECTCASE RANDDATUI(2, 20)
			CASE 0
				PRINTFORMW 「자, 봐 줘. 내 알몸은 어때…?」
			CASE 1
				PRINTFORMW 「알몸에 양말을 입은게\@ AFFECTION("상사상애") ? 좋았을까?  # %CNAME(MASTER, TARGET)%의 기호였을까. \@」
				PRINTFORM 「아무튼…, 이제 벗어 버린건 어쩔수 없어
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	PRINTFORMW 「 이제 %RAND_SPLIT("여분/안녕")%것은 필요하지 않아」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·만p FIRSTDATUI(30)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·이번탈의로성기가 보이도록(듯이)되었다경우에 표시
;·팬티 보이고를 경유하지 않고 단번에 벗는 경우 있어
;------------------------------------------------
IF 구상기입체크(100) && TFLAG:자주탈의 && P_DATUICHECK(0) == 3 && !IS_NOWASSIACTNAME(, "탈의") && !TEQUIP:눈가리개
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(100) && TALENT:TARGET:주인님 && FIRSTDATUI(30)
		PRINTFORMW 「어때? 누구에게도 보인 적 없는 곳이야.」
		PRINTFORMW 「의미를 눈치채지 못할 정도로,%CNAME(MASTER, TARGET)%가 바보가 아니였으면 좋겠네.」
		RETURN 1
	ENDIF
	
	;=================================================
	;벌써가슴가 보이고 있다
	;=================================================
	IF 구상기입체크(100) && B_DATUICHECK(0) == 1
		PRINTFORMW 「그런 눈으로 본다면…」
		PRINTFORM 「이쪽도 보여주지 않을 수가 없네…
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	;=================================================
	;응 이렇게 p
	;=================================================
	IF 구상기입체크(100) && PENIS(TARGET)
		SELECTCASE RANDDATUI(2, 30)
			CASE 0
				PRINTFORMW 「자 봐줘……」
				PRINTFORMW 「이제%CNAME(MASTER, TARGET)%(을)를 괴롭히고 싶어서, 이렇게 되어버렸어」
			CASE 1
				PRINTFORMW 「자, 잘 봐」
				PRINTFORM 「이제%CNAME(MASTER, TARGET)%가 어떻게 될까, 상상하니까 이렇게 되어 버렸어…
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		;페이드 아웃
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ AFFECTION("상사상애") ? ふふふ # フフフ \@…」")
		CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ AFFECTION("상사상애") ? 후후후 # 후후후 \@…」")
		CLEARLINE 1
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	PRINTFORMW 「이봐요, 사양하지 말아줘」
	PRINTFORM 「 좀 더 봐도 좋아…
	CALL PRINT_SYMBOL_K14
	RETURN 1
ENDIF

;·이번탈의로 젖가슴이 보이도록(듯이)되었다경우에 표시
;·브라 보이고를 경유하지 않고 단번에 벗는 경우 있어
;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·말하는 p FIRSTDATUI(40)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상기입체크(100) && TFLAG:자주탈의 && B_DATUICHECK(0) == 3 && !IS_NOWASSIACTNAME(, "탈의") && !TEQUIP:눈가리개
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(100) && TALENT:TARGET:주인님 && !TALENT:거유 && FIRSTDATUI(40)
		PRINTFORMW 「크다고 말할 정도는 아니지만, 모양은 자신 있거든… ?」
		PRINTFORMW 「어머, 보는걸 거부하진 않아. 좀 더 봐도 좋아」
		PRINTFORMW 「이런 내 모습을 볼 수 있다는 것에, 감사하라구!」
		PRINTFORMDW %CALLNAME%(은)는 자랑스럽게 말했다…
		RETURN 1
	ENDIF
	
	;=================================================
	;벌써성기가 보이고 있는	50%
	;=================================================
	IF 구상기입체크(50) && P_DATUICHECK(0) == 1
		PRINTFORMW 「사쿠야…, 내 가슴은 어때?」
		RETURN 1
	ENDIF
	
	;ONCE로 표시했다등 이후 그조교중에서는 표시시키지 않다
	;=================================================
	;조교중첫회
	;=================================================
	IF 구상기입체크(100) && ONCEDATUI(40)
		IF 0
		;조교자가거유
		ELSEIF TALENT:거유
			PRINTFORM 「\@ TALENT:MASTER:작은체형 ? 반짝반짝 눈을 빛내는게 귀여워 # 눈빛이 야하네 \@…
			CALL PRINT_SYMBOL_K14
			PRINTFORMW 「큰 가슴을 좋아하는 사람은, 아이 같은 면이 있는 거 같아.」
			;조교대상가작은체형
			SIF TALENT:MASTER:작은체형
				PRINTFORMW 「라는 건,%CNAME(MASTER, TARGET)%…, 아직 아이였어. 후후…」
		;조교자가빈유
		ELSEIF TALENT:빈유
			PRINTFORMW 「작은 가슴이 좋다니, 진짜%CNAME(MASTER, TARGET)%도 괴짜군요?」
			PRINTFORMW 「자, 이걸 보고 싶은거지?」
		ELSE
			PRINTFORMW 「머뭇거리지 말고, 어차피 볼거라면 당당하게 보는것도 좋은걸?」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%가 무엇을 생각하는지, 다 알고 있어.」
			;1/3의 확률로 진
			SIF !RAND:3
				PRINTFORMW 「본다고 해서 닳는건 아니니까 말이야.」
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·팬티 보이고 FIRSTDATUI(50)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·이번탈의로팬티（캐릭터에 의해드로와즈·트렁크）가 보이도록(듯이)되었다경우에 표시
;·하반신속옷가 없는 캐릭터는 쓰는필요없음
;------------------------------------------------
IF 구상기입체크(100) && TFLAG:자주탈의 && P_DATUICHECK(0) == 2 && !IS_NOWASSIACTNAME(, "탈의") && !TEQUIP:눈가리개
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(100) && TALENT:TARGET:주인님 && FIRSTDATUI(5)
		PRINTFORMW 「어때? 이거\@ KFLAG:TARGET:300 == 2 ? 도 # , \@내가 직접 만든 거야」
		PRINTFORMW 「사람에 따라서는 속옷 모습을 좋아하기도 한다는데…」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%는 어떤걸 더 좋아하는거야?」
		KFLAG:TARGET:300 = 1
		RETURN 1
	ENDIF
	
	;ONCE로 표시했다등 이후 그조교중에서는 표시시키지 않다
	;=================================================
	;조교중첫회
	;=================================================
	IF 구상기입체크(100) && ONCEDATUI(50)
		;2/3로 진
		IF RAND:3
			PRINTFORMW 「응, 어떄 ?」
			PRINTFORM 「이번에는,
			CALL WHICH_LINGERIE(COLOR2_K14("ALICE"))
			PRINTFORMW (으)로 했는데」
			PRINTFORMW 「자, 어떄!? 무언가%RAND_SPLIT("멋이 있는 칭찬말/감상")%의 하나 정돈,%RAND_SPLIT("말하세요!/말할 수 있지?")%」
		;1/3
		ELSE
			PRINTFORMW 「응, 이것 봐…」
			PRINTFORMW 「 이제 %RANDTEXT_K14("번지고있는", "조금 젖어있는")%데, 알고 있으려나?」
			;1/3로 진
			IF !RAND:3
				PRINTFORM 「%CNAME(MASTER, TARGET)%(을)를 괴롭힐 생각에, 이렇게 되어버렸는걸…
				CALL PRINT_SYMBOL_K14
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「フフ…」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「후후…」")
				CLEARLINE 1
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·브라 보이고 FIRSTDATUI(60)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·이번탈의로 브라（캐릭터에 의해스포츠브라·사라시）가 보이도록(듯이)되었다경우에 표시
;·상반신속옷가 없는 캐릭터는 쓰는필요없음
;------------------------------------------------
IF 구상기입체크(100) && TFLAG:자주탈의 && B_DATUICHECK(0) == 2 && !IS_NOWASSIACTNAME(, "탈의") && !TEQUIP:눈가리개
	;=================================================
	;처음
	;=================================================
	IF 구상기입체크(100) && TALENT:TARGET:주인님 && FIRSTDATUI(60)
		PRINTFORMW 「어떄, 꽤 사랑스러운 브라지? 이것\@ KFLAG:TARGET:300 == 1 ? 도 # , \@내가 직접 만든거야」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%도 한 세트 만들어줄까?」
		KFLAG:TARGET:300 = 2
		RETURN 1
	ENDIF
	
	;ONCE로 표시했다등 이후 그조교중에서는 표시시키지 않다
	;=================================================
	;조교중첫회
	;=================================================
	IF 구상기입체크(100) && ONCEDATUI(60)
		PRINTFORM 「어때?  \@ TIME ? 오늘 밤 # 오늘 \@은
		CALL WHICH_LINGERIE(COLOR2_K14("ALICE"))
		PRINTFORMW 브라를 입었어」
		SELECTCASE TENSION(TARGET, 8)
			CASE "높다"
				PRINTFORMW 「이거, 그래도 내 야심작인데」
				PRINTFORMW 「자, 뭘 망설이는거야? 칭찬한다면 지금 뿐이야!」
			CASE "일반"
				PRINTFORMW 「마음에 드는 거라고 해야 할까…」
				PRINTFORMW 「입다물지 말고, 말할 게 있다면 말해」
			CASE "낮다"
				PRINTFORMW 「…분명 말하려다 말았는데. 뭔가 말하려고 했었지?」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의·전라에 FIRSTDATUI(70)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·「전라까지 벗었다」만이 아니고, 「지금 벗었으므로전라되었다」라고 하는 의미
;·「겉옷 밖에 남지 않은상태로겉옷를 벗었다」등도 포함된다
;------------------------------------------------
IF 구상기입체크(100) && TFLAG:자주탈의 && ZENRACHECK(0) && !IS_NOWASSIACTNAME(, "탈의")
	IF AFFECTION("상사상애") && !TEQUIP:눈가리개
		PRINTFORMW 「자, 나를 봐줘… 내 몸, 깨끗하지…?」
	ELSE
		PRINTFORMW 「, 준비는OK, 라는 걸까.」
	RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·악세서리─마셔 FIRSTDATUI(80)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·「전라까지 벗었다」만이 아니고, 「지금 벗었으므로전라되었다」라고 하는 의미
;·「겉옷 밖에 남지 않은상태로겉옷를 벗었다」등도 포함된다
;------------------------------------------------
IF 구상기입체크(100) && TFLAG:강제탈의 == 6
	;=================================================
	;전라되었다
	;=================================================
	IF 구상기입체크(100) && ZENRACHECK(1)
		PRINTFORMW 「이제 이걸로 알몸이네.」
		PRINTFORMW 「자, 부끄럽다고 숨기지 마. 손을 뒤로 해서, 내게 잘 보이게%RAND_SPLIT("해/해줘")%」
		PRINTL
		SELECTCASE RANDDATUI(2, 80)
			CASE 0
				PRINTFORMW 「부끄러워하지 말고, 허리를 펴. 그쪽이 더 보기 좋아」
				PRINTFORMW 「 나 정도는 아니지만%CNAME(MASTER, TARGET)%의 알몸…, 꽤 깨끗하니까 말이지?」
			CASE 1
				PRINTFORMW 「그렇게 부끄러워하고 있는%CNAME(MASTER, TARGET)%를 보면 조금 두근두근해버려서」
				;TALENT:새드보다 ALI로 본 (분)편이 보다 효율적인 생각은 들므로
				PRINTFORMW 「마음껏\@ ALI(0) ? 귀여워해 주고 싶게 # 괴롭히고 싶게 \@되어버린걸」
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ ALI(0) ? うふふ # ウフフ \@…」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ ALI(0) ? 우후후 # 우후후 \@…」")
				CLEARLINE 1
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;그 외
	;-------------------------
	PRINTFORMW 「,%RAND_SPLIT("여분/방해")%것은 제외해 주세요」
	RETURN 1
ENDIF

