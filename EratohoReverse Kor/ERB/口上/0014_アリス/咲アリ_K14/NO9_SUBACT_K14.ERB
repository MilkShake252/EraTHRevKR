;ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·그대로응 보여라 FIRSTDATUI(100)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·이번탈의로 여자성기가 보이도록(듯이)되었다경우에 표시
;·팬티 보여라를 경유하지 않고 단번에 벗게 하는 경우 있어
;------------------------------------------------
IF 구상기입체크(100) && TFLAG:강제탈의 && P_DATUICHECK(1) == 3 && !TALENT:MASTER:남자
	;-----------------------------------
	;RETURN 사이에 두지 않고, 종단까지 일련의 처리
	;-----------------------------------
	;상냥하게 벗긴다（호감도 1000 이상·얼라이먼트정）
	IF AFFECTION("권애") && ALI(0)
		PRINTFORMW 「자, 벗겨줄게. %RAND_SPLIT("얌전히/가만히")% 있어줄래?」
		PRINTFORMW 「다리좀 올려 줄 수 있을까? 그래, 그렇게. 좋아…」
		PRINTL
	ENDIF
	PRINTFORM 「자 벗긴다…?
	CALL PRINT_SYMBOL_K14
	SELECTCASE MOIST_K14(MASTER)
		CASE "높다"
			PRINTFORMW 「뭐야, 벌써 이렇게 젖어버린 걸까.%RAND_SPLIT("야한/추잡한")%%RAND_SPLIT("아가씨/사람이")%구나?」
		CASE "일반"
			PRINTFORMW 「후후,%RAND_SPLIT("이제/약간")%젖어 버린것 %RAND_SPLIT("같/처럼 보이")%네」
		CASE "낮다"
			PRINTFORMW 「어머, 아직 젖지도 않은거 같네?」
	ENDSELECT
	PRINTFORMW 「%RAND_SPLIT("자/자아")%, 이번에는 어떻게\@ ALI(0) ? 귀여워해 # 괴롭혀 \@줄까」
	;페이드 인
	CALL FADEIN(13, "「…기대되네」")
	;페이드 아웃
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%RAND_SPLIT("ウフフ/うふふ")%…」")
	CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%RAND_SPLIT("우후후/우후후")%…」")
	CLEARLINE 1
	;종단으로 RETURN
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·젖가슴 보여라 FIRSTDATUI(110)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·이번탈의로 젖가슴이 보이도록(듯이)되었다경우에 표시
;·브라 보여라를 경유하지 않고 단번에 벗게 하는 경우 있어
;·조교대상이남자라고 표시하지 않기 때문에, 남자한정구상에서는 쓰는필요가 없다
;------------------------------------------------
IF 구상기입체크(100) && TFLAG:강제탈의 && B_DATUICHECK(1) == 3 && !TALENT:MASTER:남자
	LOCALS = \@ FIRSTDATUI(110) ? 이렇게 해 보면, 정말 x0020># 언제 봐도 정말로 \@
	PRINTFORM 「,%CNAME(MASTER, TARGET)%의 가슴 보여줘…
	CALL PRINT_SYMBOL_K14
	PRINTFORML 「%LOCALS%깨끗한 %RAND_SPLIT("형태/가슴이")%네」
	IF 0
	;조교자가거유
	ELSEIF TALENT:거유
		PRINTFORMW 「%RAND_SPLIT("뭐, 나만큼 크면 더 좋았을텐데?/너무 크면 감도도 나빠지고, 나, 원래대로 되돌릴까…")%」
	;조교자가빈유
	ELSEIF TALENT:빈유
		PRINTFORMW 「%RAND_SPLIT("나보다 큰데다가 건방진 가슴…아, 이건 농담이야. 원래대로 되돌리면 되는 이야기니까/뭐, 그래도, 감도가 좋다는 점에서는 지지 않을 생각이지만!")%」
	;보통
	ELSE
		PRINTFORMW 「%RAND_SPLIT("그렇다고 해도…나만큼은 아니지만/역시 이 정도가 가장 좋은거야")%. 후후…」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·팬티 보여라 FIRSTDATUI(120)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·이번탈의로하반신속옷가 보이도록(듯이)되었다경우에 표시
;·조교대상가하반신속옷가 없는 캐릭터 한정의구상이면 쓰는필요가 없다
;------------------------------------------------
IF 구상기입체크(100) && TFLAG:강제탈의 && P_DATUICHECK(1) == 2
	;공통으로 처리했다 있고 부분이 있던 것으로, 테두리를 나누지 않고 정리해 버렸다고 한다. 나눈 (분)편이 겉모습 깨끗이 하지만
	PRINTFORMW 「%RAND_SPLIT("후후/후후")%…좋은 모습이야. 다른 사람한테는 보여주지 않겠지? 이런%RAND_SPLIT("상스러운/흐트러진")%속옷 모습」
	PRINTFORM 「부끄러운 듯이 고개를 숙이고 있는 모습도 %RAND_SPLIT("귀엽네?/참을 수 없네요…")%　\@ ALI(0) ? 조금 # 더욱 \@심술궃게 되고 싶어지는걸
	CALL PRINT_SYMBOL_K14
	;조교대상윤활파생
	SELECTCASE MOIST_K14(MASTER)
		CASE "높다"
			PRINTFORMW 「…어머, 벌써 흘러내리고 있잖아」
			PRINTFORMW 「욱신거리는거 괴롭지 않아? 내가 %RAND_SPLIT("엉망진창으로 해줬으면/괴롭혀주면/휘저어 줬으면")% 하는 거지?」
			PRINTFORMW 「자, 솔직하게 이야기해줬으면 좋겠어…」
		CASE "일반"
			PRINTFORMW 「응?  %CNAME(MASTER, TARGET)%…, 벌써 젖어버린 것 같%RAND_SPLIT("은데/네")%?」
			PRINTFORMW 「이제부터 내가 할 일을%RAND_SPLIT("생각한/기대한")%것 같네」
			PRINTFORMW 「저기, 그렇지? 그런 거지? 내눈을 보고 똑바로 대답해줘…」
		CASE "낮다"
			PRINTFORMW 「어머나? 아직 젖지 않았구나. %RAND_SPLIT("재미없는/시시한/따분한")% 이야기야」
			PRINTFORMW 「%RAND_SPLIT("혹시 사쿠야, 당신…, 젖기 어려운 체질?/이 정도론 당신…, 흥분하지 않는 걸까? 귀찮은 일이네")%」
	ENDSELECT
	;1/3의확률로표시 한다
	IF !RAND:3
		;페이드 아웃
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%RAND_SPLIT("クスクス/쿠쿡/フフフ/ウフフ")%…」")
		CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%RAND_SPLIT("킥킥/쿡쿡/후후후/우후후")%…」")
		;행 삭제
		CLEARLINE 1
	ENDIF
	;종단에서만 RETURN
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·브라 보여라 FIRSTDATUI(130)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·이번탈의로 브라상반신속옷가 보이도록(듯이)되었다경우에 표시
;·조교대상이남자라고 표시하지 않기 때문에, 남자한정구상에서는 쓰는필요가 없다
;·조교대상가상반신속옷가 없는 캐릭터 한정의구상이면 쓰는필요가 없다
;------------------------------------------------
IF 구상기입체크(100) && TFLAG:강제탈의 && B_DATUICHECK(1) == 2 && !TALENT:MASTER:남자
	;FIRSTDATUI를 숙청해 ORDERDATUI에 통합
	SELECTCASE ORDERDATUI(130)
		CASE 0
			PRINTFORMW 「어머나, 귀여운 속옷? 사쿠야 치곤 나쁘지 않은 취미네요. 후후」
			PRINTFORMW 「단지…, 좀 더 어른스러운 느낌의 속옷이 당신에게는 어울릴지도」
			PRINTFORMW 「조만간 내가 맞춰줄게.%CNAME(MASTER, TARGET)%가 얌전히 어울려준다면, 말이야」
			KFLAG:(FLAG:주인님):310 = 1
		CASE 1
			IF 0
			ELSEIF TALENT:MASTER:거유
				PRINTFORMW 「%RAND_SPLIT("조금/언제 봐도")% 답답해 보이는걸?」
				PRINTFORMW 「조금 더 여유있는 사이즈로 해도 괜찮을지도 몰라」
			ELSEIF TALENT:MASTER:빈유
				PRINTFORMW 「컵도 언더도 조금 느슨한 느낌이야. 그러면 축 쳐질지도…」
				PRINTFORMW 「크기에 관계없이 사이즈가 맞는 걸로 하는게 좋을껄?」
			ELSE
				PRINTFORMW 「후후, 잘 어울리네. 하지만, 그래…」
				PRINTFORMW 「사이즈는 계속 바뀌게 되니까, 거기에 맞춰서 바꿔야지.」
				PRINTFORMW 「그것이 아름다움을 유지하는 비결. 잘 기억해 둬」
			ENDIF
		CASE 2
			PRINTFORMW 「응, 나쁘지 않아. 다만, 역시%CNAME(MASTER, TARGET)%의 경우,\@ TALENT:MASTER:거유 ? 풀 # %RANDTEXT_K14("1/2", "3/4")% \@컵쪽이 좋을지도 모르겠어」
			PRINTFORMW 「기억해 두는게 좋아.」
		CASE 3
			PRINTFORMW 「어머, 괜찮은 느낌? 사이즈도 딱 맞는것 같고」
			PRINTFORMW 「그래도… 그래, 잘못된 방법은 버스트 라인을 무너뜨리니까, 착용하는 방법도 중요해.」
			PRINTFORMW 「예를 들면, 우선…상체를 90도로 눕히고, 가슴을 컵 안에 담아두고, 그 다음…」
			PRINTFORMDW 당돌하게 시작된 %CALLNAME%에 의한 착용 강좌는, 그 후로도 계속되었다…
		;범용 처리
		CASEELSE
			SELECTCASE RANDDATUI(2, 13)
				CASE 0
					PRINTFORMW 「내 말을 잘 듣는걸 보면 정신차리고 있는거 같네?  %RAND_SPLIT("좋은 일/다행")%(이)예요」
					PRINTFORM 「그래, 이번
					CALL WHICH_LINGERIE(COLOR2_K14("ALICE"))
					PRINTFORMW 어떻게 시험해 볼래?」
				CASE 1
					PRINTFORMW 「\@ ALI(0) ? 후후 # 후후 \@, 귀여워. 어울리네.」
					PRINTFORM 「벗%RAND_SPLIT("기는 것이/어 버리는것이")% 아까울 정도로 말이야.
					CALL PRINT_SYMBOL_K14
			ENDSELECT
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의·전라에 FIRSTDATUI(140)
;━━━━━━━━━━━━━━━━━━━━━━━━
;·「전라까지 벗겼다」만이 아니고, 「지금 벗겼으므로전라되었다」라고 하는 의미
;·「겉옷 밖에 남지 않은상태로겉옷를 벗겼다」등도 포함된다
;·「젖가슴 보여라」～「팬티 보여라」를 썼을 경우는 기입하는필요는 없다（상층의 분기에 먹혀져 여기까지 떨어져 오지 않기 때문에）
